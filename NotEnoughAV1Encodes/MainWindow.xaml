<mah:MetroWindow x:Class="NotEnoughAV1Encodes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:mah="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
        xmlns:local="clr-namespace:NotEnoughAV1Encodes"
        mc:Ignorable="d"
        Title="NotEnoughAV1Encodes" Height="650" Width="1085" ResizeMode="CanResizeWithGrip" AllowDrop="True" DragDrop.Drop="Grid_Drop">
    <Window.Resources>
        <BooleanToVisibilityConverter x:Key="b2v"/>
    </Window.Resources>
    <Window.TaskbarItemInfo>
        <TaskbarItemInfo ProgressState="Normal"/>
    </Window.TaskbarItemInfo>
    <Grid>
        <Image x:Name="imgDynamic" Stretch="UniformToFill"/>
        <Button x:Name="ButtonOpenSource" Margin="21,20,0,0" VerticalAlignment="Top" Height="48" HorizontalAlignment="Left" Width="138" Background="{x:Null}" Click="ButtonOpenSource_Click" BorderBrush="#FF707070">
            <Canvas HorizontalAlignment="Left" Height="40" VerticalAlignment="Top" Width="100">
                <Image HorizontalAlignment="Left" Height="51" VerticalAlignment="Top" Width="48" Source="img/video1.png" Canvas.Top="-7" Canvas.Left="-4"/>
                <Label Content="Source" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.463,0.491" Canvas.Left="44" Height="30" Canvas.Top="2" FontSize="13" />
            </Canvas>
        </Button>
        <mah:MetroAnimatedSingleRowTabControl x:Name="MetroTab" Margin="21,116,21,63" Background="{x:Null}">
            <mah:MetroTabItem Header="Summary">
                <Grid x:Name="Grid0">
                    <GroupBox Header="Current Encoding Settings" Margin="20,192,19,0" Height="82" VerticalAlignment="Top">
                        <Grid>
                            <Label Content="Encoder:" HorizontalAlignment="Left" Margin="7,13,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <Label Content="{Binding Text, ElementName=ComboBoxVideoEncoder, UpdateSourceTrigger=PropertyChanged}"  HorizontalAlignment="Left" Margin="71,13,0,0" VerticalAlignment="Top" Width="74"/>
                            <Label Content="Bit-Depth:" HorizontalAlignment="Left" Margin="150,13,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <Label Content="{Binding Text, ElementName=ComboBoxVideoBitDepth, UpdateSourceTrigger=PropertyChanged}"  HorizontalAlignment="Left" Margin="244,13,0,0" VerticalAlignment="Top" Width="40"/>
                            <Label Content="Speed:" HorizontalAlignment="Left" Margin="326,13,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <Label Content="{Binding Value, ElementName=SliderVideoSpeed, UpdateSourceTrigger=PropertyChanged}"  HorizontalAlignment="Left" Margin="408,13,0,0" VerticalAlignment="Top" Width="46"/>
                            <Label Content="Passes:" HorizontalAlignment="Left" Margin="488,13,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <Label Content="{Binding Text, ElementName=ComboBoxVideoPasses, UpdateSourceTrigger=PropertyChanged}"  HorizontalAlignment="Left" Margin="567,13,0,0" VerticalAlignment="Top" Width="140"/>
                        </Grid>
                    </GroupBox>
                    <GroupBox Header="Video Information" Height="72" Margin="20,115,19,0" VerticalAlignment="Top">
                        <Grid>
                            <Label Content="Length:" HorizontalAlignment="Left" Margin="10,5,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <Label x:Name="LabelVideoLength" Content="00:00:00" HorizontalAlignment="Left" Margin="67,5,0,0" VerticalAlignment="Top" Width="80"/>
                            <Label Content="Color Format:" HorizontalAlignment="Left" Margin="147,5,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <Label x:Name="LabelVideoColorFomat" Content="yuv420p" HorizontalAlignment="Left" Margin="239,5,0,0" VerticalAlignment="Top" Width="80"/>
                            <Label Content="Framerate:" HorizontalAlignment="Left" Margin="324,5,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <Label x:Name="LabelVideoFramerate" Content="24" HorizontalAlignment="Left" Margin="400,5,0,0" VerticalAlignment="Top" Width="80"/>
                            <Label Content="Resolution:" HorizontalAlignment="Left" Margin="485,5,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <Label x:Name="LabelVideoResolution" Content="1920x1080" HorizontalAlignment="Left" Margin="563,5,0,0" VerticalAlignment="Top" Width="80"/>

                        </Grid>
                    </GroupBox>
                    <GroupBox Header="Input / Output" Height="100" Margin="20,10,19,0" VerticalAlignment="Top">
                        <Grid HorizontalAlignment="Left" Height="66" Margin="0,0,0,-1" VerticalAlignment="Top" Width="813">
                            <Label Content="Source:" HorizontalAlignment="Left" Margin="10,1,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <Label Content="Destination:" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <TextBox x:Name="TextBoxVideoSource" Height="29" Margin="105,1,8,0" TextWrapping="Wrap" Text="No Source Selected" VerticalAlignment="Top" FontStyle="Italic" VerticalContentAlignment="Center" IsReadOnly="True" BorderBrush="{x:Null}" Background="{x:Null}"/>
                            <TextBox x:Name="TextBoxVideoDestination" Height="29" Margin="105,30,8,0" TextWrapping="Wrap" Text="No Destination Selected" VerticalAlignment="Top" FontStyle="Italic" VerticalContentAlignment="Center" IsReadOnly="True" BorderBrush="{x:Null}" Background="{x:Null}"/>
                        </Grid>
                    </GroupBox>
                    <GroupBox Header="Workers &amp; Splitting" Height="85" Margin="20,279,19,0" VerticalAlignment="Top">
                        <Grid HorizontalAlignment="Left" Height="46" VerticalAlignment="Top" Width="986">
                            <Label Content="Worker Count:" HorizontalAlignment="Left" Margin="9,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <ComboBox x:Name="ComboBoxWorkerCount" HorizontalAlignment="Left" Margin="106,10,0,0" VerticalAlignment="Top" Width="80" Height="29" ToolTip="Amount of parallel workers."/>
                            <Label Content="Priority:" HorizontalAlignment="Left" Margin="202,10,0,0" VerticalAlignment="Top" FontWeight="Bold" Height="28" RenderTransformOrigin="0.393,0.581"/>
                            <ComboBox x:Name="ComboBoxProcessPriority" HorizontalAlignment="Left" Margin="258,10,0,0" VerticalAlignment="Top" Width="100" Height="29">
                                <ComboBoxItem Content="Normal" IsSelected="True"/>
                                <ComboBoxItem Content="Below Normal"/>
                            </ComboBox>
                            <Label Content="Splitting Method:" HorizontalAlignment="Left" Margin="370,11,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                            <ComboBox x:Name="ComboBoxSplittingMethod" HorizontalAlignment="Left" Margin="485,10,0,0" VerticalAlignment="Top" Width="120" Height="28" VerticalContentAlignment="Center" SelectionChanged="ComboBoxSplittingMethod_SelectionChanged" ToolTip="Splitting Method" SelectedIndex="0">
                                <ComboBoxItem Content="Scene Detect (ffmpeg)" IsSelected="True"/>
                                <ComboBoxItem Content="PySceneDetect"/>
                                <ComboBoxItem Content="Equal Chunking"/>
                            </ComboBox>
                            <Label Content="Threshold:" HorizontalAlignment="Left" Margin="616,10,0,0" VerticalAlignment="Top" FontWeight="Bold">
                                <Label.Style>
                                    <Style>
                                        <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="0">
                                                <Setter Property="UIElement.Visibility" Value="Visible"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Label.Style>
                            </Label>
                            <TextBox x:Name="TextBoxSplittingThreshold" PreviewTextInput="NumberValidationTextBox" HorizontalAlignment="Left" Height="26" Margin="690,10,0,0" TextWrapping="Wrap" Text="0.3" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" ToolTip="Scene Detect Threshold">
                                <TextBox.Style>
                                    <Style>
                                        <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="0">
                                                <Setter Property="UIElement.Visibility" Value="Visible"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </TextBox.Style>
                            </TextBox>
                            <CheckBox x:Name="CheckBoxSplittingReencode" Content="Re-Encode:" HorizontalAlignment="Left" Margin="616,17,0,0" VerticalAlignment="Top" IsChecked="True" RenderTransformOrigin="0.439,0.467" FontWeight="Bold">
                                <CheckBox.Style>
                                    <Style>
                                        <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="2">
                                                <Setter Property="UIElement.Visibility" Value="Visible"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </CheckBox.Style>
                            </CheckBox>
                            <ComboBox x:Name="ComboBoxSplittingReencodeMethod" HorizontalAlignment="Left" Margin="707,10,0,0" VerticalAlignment="Top" Width="81" Height="28" VerticalContentAlignment="Center" RenderTransformOrigin="0.498,0.488">
                                <ComboBox.Style>
                                    <Style>
                                        <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="2">
                                                <Setter Property="UIElement.Visibility" Value="Visible"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </ComboBox.Style>
                                <ComboBoxItem Content="x264" IsSelected="True"/>
                                <ComboBoxItem Content="ffv1"/>
                                <ComboBoxItem Content="utvideo"/>
                            </ComboBox>
                            <Label Content="Chunk-Length (sec):" HorizontalAlignment="Left" Margin="793,10,0,0" VerticalAlignment="Top" FontWeight="Bold">
                                <Label.Style>
                                    <Style>
                                        <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="2">
                                                <Setter Property="UIElement.Visibility" Value="Visible"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Label.Style>
                            </Label>
                            <TextBox x:Name="TextBoxSplittingChunkLength" PreviewTextInput="NumberValidationTextBox" HorizontalAlignment="Left" Height="27" Margin="921,10,0,0" TextWrapping="Wrap" Text="10" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">
                                <TextBox.Style>
                                    <Style>
                                        <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="2">
                                                <Setter Property="UIElement.Visibility" Value="Visible"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </TextBox.Style>
                            </TextBox>
                        </Grid>
                    </GroupBox>
                </Grid>
            </mah:MetroTabItem>
            <mah:MetroTabItem Header="Trim">
                <Grid Background="{Binding ElementName=Grid0, Path=Background, UpdateSourceTrigger=PropertyChanged}">
                    <GroupBox Header="Trim End" HorizontalAlignment="Left" Height="250" Margin="407,63,0,0" VerticalAlignment="Top" Width="360">
                        <Image x:Name="ImagePreviewTrimEnd" Margin="0,0,0,-2" HorizontalAlignment="Right" Stretch="Fill" Width="350" Source="img\offline.png"/>
                    </GroupBox>
                    <TextBox x:Name="TextBoxTrimEnd" HorizontalAlignment="Left" Height="26" Margin="538,63,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="101" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsTabStop="False" Text="00:23:00.000" TextChanged="TextBoxTrimEnd_TextChanged"/>
                    <Button x:Name="ButtonTrimPlusEnd" Content="+" HorizontalAlignment="Left" Margin="639,63,0,0" VerticalAlignment="Top" Width="25" Height="26" Click="ButtonTrimPlusEnd_Click" />
                    <Button x:Name="ButtonTrimMinusEnd" Content="-" HorizontalAlignment="Left" Margin="513,63,0,0" VerticalAlignment="Top" Width="25" Height="26" Click="ButtonTrimMinusEnd_Click"  />
                    <Label Content="(in seconds)" HorizontalAlignment="Left" Margin="265,107,0,0" VerticalAlignment="Top" FontStyle="Italic">
                        <Label.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="2">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Label.Style>
                    </Label>
                    <GroupBox Header="Trim Start" HorizontalAlignment="Left" Height="250" Margin="34,63,0,0" VerticalAlignment="Top" Width="360">
                        <Image x:Name="ImagePreviewTrimStart" Height="216" VerticalAlignment="Top" HorizontalAlignment="Right" Width="350" Stretch="Fill" Source="img/offline.png" Margin="0,0,0,-1"/>
                    </GroupBox>
                    <TextBox x:Name="TextBoxTrimStart"  HorizontalAlignment="Left" Height="26" Margin="165,63,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsTabStop="False" Text="00:00:00.000" TextChanged="TextBoxTrimEnd_TextChanged" />
                    <Button x:Name="ButtonTrimPlus" Content="+" HorizontalAlignment="Left" Margin="265,63,0,0" VerticalAlignment="Top" Width="24" Height="26" Click="ButtonTrimPlus_Click"/>
                    <Button x:Name="ButtonTrimMinus" Content="-" HorizontalAlignment="Left" Margin="139,63,0,0" VerticalAlignment="Top" Width="26" Height="26" Click="ButtonTrimMinus_Click"/>
                    <Label Content="(Soft Subs not supported)" HorizontalAlignment="Left" Margin="322,37,0,0" VerticalAlignment="Top" Foreground="Red" FontWeight="Bold"/>
                    <mah:ToggleSwitch x:Name="ToggleSwitchTrimming" Header="" Toggled="CheckBoxTrimming_Checked" HorizontalAlignment="Left" Height="56" Margin="34,2,0,0" VerticalAlignment="Top" Width="100"/>
                </Grid>
            </mah:MetroTabItem>
            <mah:MetroTabItem Header="Filters">
                <Grid Background="{Binding ElementName=Grid0, Path=Background, UpdateSourceTrigger=PropertyChanged}">
                    <GroupBox Header="Crop" HorizontalAlignment="Left" Height="203" Margin="14,20,0,0" VerticalAlignment="Top" Width="274">
                        <Grid IsEnabled="{Binding IsOn, ElementName=ToggleSwitchFilterCrop}">
                            <TextBox x:Name="TextBoxFiltersCropLeft" PreviewTextInput="NumberValidationTextBox" HorizontalAlignment="Left" Height="26" Margin="52,70,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                            <TextBox x:Name="TextBoxFiltersCropRight" PreviewTextInput="NumberValidationTextBox" HorizontalAlignment="Left" Height="26" Margin="163,70,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                            <TextBox x:Name="TextBoxFiltersCropTop" PreviewTextInput="NumberValidationTextBox" HorizontalAlignment="Left" Height="26" Margin="108,38,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                            <TextBox x:Name="TextBoxFiltersCropBottom" PreviewTextInput="NumberValidationTextBox" HorizontalAlignment="Left" Height="26" Margin="108,102,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                            <Label Content="Bottom" HorizontalAlignment="Left" Margin="108,128,0,0" VerticalAlignment="Top" Width="55"/>
                            <Label Content="Top" HorizontalAlignment="Left" Margin="120,8,0,0" VerticalAlignment="Top"/>
                            <Label Content="Left" HorizontalAlignment="Left" Margin="19,70,0,0" VerticalAlignment="Top" Height="26"/>
                            <Label Content="Right" HorizontalAlignment="Left" Margin="218,68,0,0" VerticalAlignment="Top" Height="30"/>
                        </Grid>
                    </GroupBox>
                    <GroupBox Header="Resize" HorizontalAlignment="Left" Height="203" Margin="294,20,0,0" VerticalAlignment="Top" Width="285" >
                        <Grid IsEnabled="{Binding IsOn, ElementName=ToggleSwitchFilterResize}">
                            <Label Content="Scaling Algorithm:" HorizontalAlignment="Left" Margin="19,98,0,0" VerticalAlignment="Top" Height="28"/>
                            <ComboBox x:Name="ComboBoxFiltersScaling" HorizontalAlignment="Left" Margin="131,96,0,0" VerticalAlignment="Top" Width="128" Height="30" VerticalContentAlignment="Center">
                                <ComboBoxItem Content="fast_bilinear"/>
                                <ComboBoxItem Content="bilinear"/>
                                <ComboBoxItem Content="bicubic" IsSelected="True"/>
                                <ComboBoxItem Content="experimental"/>
                                <ComboBoxItem Content="neighbor"/>
                                <ComboBoxItem Content="area"/>
                                <ComboBoxItem Content="bicublin"/>
                                <ComboBoxItem Content="gauss"/>
                                <ComboBoxItem Content="sinc"/>
                                <ComboBoxItem Content="lanczos"/>
                                <ComboBoxItem Content="spline"/>
                            </ComboBox>
                            <Label Content="Width:" HorizontalAlignment="Left" Margin="19,48,0,0" VerticalAlignment="Top" Height="28"/>
                            <TextBox x:Name="TextBoxFiltersResizeWidth" PreviewTextInput="NumberValidationTextBox" HorizontalAlignment="Left" Height="28" Margin="71,46,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="60" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" ToolTip="If set to 0 it will automatically set the corresponding width to the height."/>
                            <Label Content="Height:" HorizontalAlignment="Left" Margin="141,48,0,0" VerticalAlignment="Top" Height="28"/>
                            <TextBox x:Name="TextBoxFiltersResizeHeight" PreviewTextInput="NumberValidationTextBox" HorizontalAlignment="Left" Height="28" Margin="199,46,0,0" TextWrapping="Wrap" Text="1080" VerticalAlignment="Top" Width="60" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                            <Label Content="0 = Auto" HorizontalAlignment="Left" Margin="71,67,0,0" VerticalAlignment="Top" FontStyle="Italic" RenderTransformOrigin="0.504,0.51" Width="60"/>
                        </Grid>
                    </GroupBox>
                    <GroupBox Header="Rotate" HorizontalAlignment="Left" Height="95" Margin="591,128,0,0" VerticalAlignment="Top" Width="285">
                        <Grid IsEnabled="{Binding IsOn, ElementName=ToggleSwitchFilterRotate}">
                            <Label Content="Rotate (Clockwise):" HorizontalAlignment="Left" Margin="36,18,0,0" VerticalAlignment="Top" Height="28"/>
                            <ComboBox x:Name="ComboBoxFiltersRotate" HorizontalAlignment="Left" Margin="151,18,0,0" VerticalAlignment="Top" Width="65" Height="29" VerticalContentAlignment="Center">
                                <ComboBoxItem Content="0°" IsSelected="True"/>
                                <ComboBoxItem Content="90°"/>
                                <ComboBoxItem Content="180°"/>
                                <ComboBoxItem Content="270°"/>
                            </ComboBox>
                        </Grid>
                    </GroupBox>
                    <GroupBox Header="Deinterlace" HorizontalAlignment="Left" Height="96" Margin="591,20,0,0" VerticalAlignment="Top" Width="285">
                        <Grid IsEnabled="{Binding IsOn, ElementName=ToggleSwitchFilterDeinterlace}">
                            <Label Content="Deinterlace (Yadif):" HorizontalAlignment="Left" Margin="39,21,0,0" VerticalAlignment="Top" Height="28"/>
                            <ComboBox x:Name="ComboBoxFiltersDeinterlace" HorizontalAlignment="Left" Margin="152,20,0,0" VerticalAlignment="Top" Width="65" Height="29" VerticalContentAlignment="Center">
                                <ComboBoxItem Content="1" IsSelected="True"/>
                                <ComboBoxItem Content="0"/>
                            </ComboBox>
                        </Grid>
                    </GroupBox>
                    <mah:ToggleSwitch x:Name="ToggleSwitchFilterCrop" Header="" Toggled="CheckBoxTrimming_Checked" HorizontalAlignment="Left" Height="61" Margin="56,-6,0,0" VerticalAlignment="Top" Width="100"/>
                    <mah:ToggleSwitch x:Name="ToggleSwitchFilterResize" Header="" Toggled="CheckBoxTrimming_Checked" HorizontalAlignment="Left" Height="100" Margin="341,-6,0,0" VerticalAlignment="Top" Width="100"/>
                    <mah:ToggleSwitch x:Name="ToggleSwitchFilterRotate" Header="" Toggled="CheckBoxTrimming_Checked" HorizontalAlignment="Left" Height="100" Margin="649,102,0,0" VerticalAlignment="Top" Width="159"/>
                    <mah:ToggleSwitch x:Name="ToggleSwitchFilterDeinterlace" Header="" Toggled="CheckBoxTrimming_Checked" HorizontalAlignment="Left" Height="100" Margin="677,-6,0,0" VerticalAlignment="Top" Width="100"/>
                </Grid>
            </mah:MetroTabItem>
            <mah:MetroTabItem Header="Video">
                <Grid Background="{Binding ElementName=Grid0, Path=Background, UpdateSourceTrigger=PropertyChanged}">
                    <GroupBox Header="Video" HorizontalAlignment="Left" Height="190" Margin="10,10,0,0" VerticalAlignment="Top" Width="385">
                        <Grid>
                            <Label Content="Encoder:" HorizontalAlignment="Left" Margin="19,15,0,0" VerticalAlignment="Top"/>
                            <ComboBox x:Name="ComboBoxVideoEncoder" HorizontalAlignment="Left" Margin="135,14,0,0" VerticalAlignment="Top" Width="128" Height="28" VerticalContentAlignment="Center" SelectionChanged="ComboBoxVideoEncoder_SelectionChanged" ToolTip="Encoder to use" SelectedIndex="0">
                                <ComboBoxItem Content="aomenc"  IsSelected="True"/>
                                <ComboBoxItem Content="rav1e"/>
                                <ComboBoxItem Content="svt-av1"/>
                                <ComboBoxItem Content="vp9"/>
                            </ComboBox>
                            <Label Content="Bit-Depth:" HorizontalAlignment="Left" Margin="19,54,0,0" VerticalAlignment="Top"/>
                            <ComboBox x:Name="ComboBoxVideoBitDepth" HorizontalAlignment="Left" Margin="135,52,0,0" VerticalAlignment="Top" Width="60" Height="30" VerticalContentAlignment="Center" ToolTip="Bit-Depth. 10bit recommended for animation and to avoid banding.">
                                <ComboBoxItem Content="8" IsSelected="True"/>
                                <ComboBoxItem Content="10"/>
                                <ComboBoxItem Content="12"/>
                            </ComboBox>
                            <Label Content="VFR:" HorizontalAlignment="Left" Margin="19,94,0,0" VerticalAlignment="Top"/>
                            <Label Content="only if video is VFR; both input and output have to be mkv" HorizontalAlignment="Left" Margin="19,113,0,0" VerticalAlignment="Top" Width="356" FontStyle="Italic"/>
                            <mah:ToggleSwitch x:Name="ToggleSwitchVFR" Header="" HorizontalAlignment="Left" Height="100" Margin="135,68,0,-13" VerticalAlignment="Top" Width="100" ToolTip="ONLY TOGGLE IF SOURCE IS VFR"/>

                        </Grid>
                    </GroupBox>
                    <GroupBox Header="Quality" Height="190" Margin="10,205,10,0" VerticalAlignment="Top">
                        <Grid>
                            <RadioButton x:Name="RadioButtonVideoConstantQuality" Content="Constant Quality:" HorizontalAlignment="Left" Margin="42,14,0,0" VerticalAlignment="Top" Height="28" RenderTransformOrigin="0.504,0.192" VerticalContentAlignment="Center" IsChecked="True" Background="{x:Null}"/>
                            <Slider x:Name="SliderVideoQuality" IsEnabled="{Binding IsChecked, ElementName=RadioButtonVideoConstantQuality}" Margin="60,48,65,0" VerticalAlignment="Top" TickPlacement="BottomRight" Maximum="63" Value="28" SmallChange="1" IsSnapToTickEnabled="True" Height="28" IsDirectionReversed="True" Foreground="#FF595959" ToolTip="Target Quality"/>
                            <Label x:Name="LabelVideoQuality" IsEnabled="{Binding IsChecked, ElementName=RadioButtonVideoConstantQuality}" Content="{Binding Value, ElementName=SliderVideoQuality, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="172,17,0,0" VerticalAlignment="Top"/>
                            <Label Content="| Lowest Quality" HorizontalAlignment="Left" Margin="58,70,0,0" VerticalAlignment="Top" FontSize="9" Width="79" Foreground="#FF595959"/>
                            <Label Content="Highest Quality |" Margin="0,76,59,0" VerticalAlignment="Top" FontSize="9" Foreground="#FF595959" RenderTransformOrigin="0.93,0.773" HorizontalAlignment="Right" Width="80"/>
                            <RadioButton x:Name="RadioButtonVideoBitrate" Content="Bitrate (kbps):" HorizontalAlignment="Left" Margin="42,109,0,0" VerticalAlignment="Top" Width="125" Height="29" VerticalContentAlignment="Center" Background="{x:Null}" ToolTip="Target Bitrate"/>
                            <TextBox x:Name="TextBoxVideoBitrate" PreviewTextInput="NumberValidationTextBox" IsEnabled="{Binding IsChecked, ElementName=RadioButtonVideoBitrate}" HorizontalAlignment="Left" Height="29" Margin="167,109,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                        </Grid>
                    </GroupBox>
                    <GroupBox Header="Optimisation" Margin="0,0,10,201" Height="190" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="385">
                        <Grid>
                            <CheckBox x:Name="CheckBoxVideoAomencRealTime" Content="Real Time Mode" HorizontalAlignment="Left" Margin="226,106,0,0" VerticalAlignment="Top" Checked="CheckBoxVideoAomencRealTime_Checked" Visibility="Visible"/>
                            <Label Content="Speed:" HorizontalAlignment="Left" Margin="17,11,0,0" VerticalAlignment="Top"/>
                            <Slider x:Name="SliderVideoSpeed" HorizontalAlignment="Left" Margin="63,43,0,0" VerticalAlignment="Top" Width="276" Height="29" TickPlacement="BottomRight" Foreground="#FF595959" Maximum="9" Value="4" SmallChange="0" IsSnapToTickEnabled="True" ValueChanged="SliderVideoSpeed_ValueChanged"/>
                            <Label x:Name="LabelVideoSpeed" Content="{Binding Value, ElementName=SliderVideoSpeed, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="68,11,0,0" VerticalAlignment="Top"/>
                            <Label Content="Passes:" HorizontalAlignment="Left" Margin="20,103,0,0" VerticalAlignment="Top"/>
                            <ComboBox x:Name="ComboBoxVideoPasses" HorizontalAlignment="Left" Margin="85,101,0,0" VerticalAlignment="Top" Width="60" Height="28" VerticalContentAlignment="Center" SelectionChanged="ComboBoxVideoPasses_SelectionChanged">
                                <ComboBoxItem Content="1" IsSelected="True"/>
                                <ComboBoxItem Content="2"/>
                            </ComboBox>
                            <Label Content="| Slowest" HorizontalAlignment="Left" Margin="61,66,0,0" VerticalAlignment="Top" FontSize="9" Width="80"/>
                            <Label Content="Fastest |" HorizontalAlignment="Left" Margin="299,66,-4,0" VerticalAlignment="Top" FontSize="9" Width="80"/>
                        </Grid>
                    </GroupBox>
                    <Label Content="Advanced Settings:" HorizontalAlignment="Left" Margin="815,10,0,0" VerticalAlignment="Top"/>
                    <mah:ToggleSwitch x:Name="ToggleSwitchAdvancedVideoSettings" Header="&#xA;" HorizontalAlignment="Left" Height="73" Margin="936,-34,0,0" VerticalAlignment="Top" Width="89" RenderTransformOrigin="0.44,0.39"/>
                </Grid>
            </mah:MetroTabItem>
            <mah:MetroTabItem Header="Advanced" Visibility="{Binding ElementName=ToggleSwitchAdvancedVideoSettings, Path=IsOn, Converter={StaticResource b2v}}">
                <Grid Background="{Binding ElementName=Grid0, Path=Background, UpdateSourceTrigger=PropertyChanged}">
                    <Label Content="Advanced Settings" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
