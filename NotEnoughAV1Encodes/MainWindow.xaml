<Window x:Name="Window" x:Class="NotEnoughAV1Encodes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="NotEnoughAV1Encodes" Height="621" Width="1008.333" ResizeMode="CanMinimize">
    <Window.Resources>
        <BooleanToVisibilityConverter x:Key="b2v" />
    </Window.Resources>
    <Window.TaskbarItemInfo>
        <TaskbarItemInfo ProgressState="Normal"/>
    </Window.TaskbarItemInfo>

    <Grid x:Name="Grid">
        <Image x:Name="imgDynamic" Stretch="UniformToFill">
        </Image>
        <Button x:Name="ButtonOpenSource" HorizontalAlignment="Left" Margin="21.333,16,0,0" VerticalAlignment="Top" Width="138" Height="47.25" HorizontalContentAlignment="Center" VerticalContentAlignment="Stretch" UseLayoutRounding="False" BorderBrush="#FFE4E4E4" Background="{Binding ElementName=TextBoxPresetName, Path=Background, UpdateSourceTrigger=PropertyChanged}" Click="ButtonOpenSource_Click">
            <StackPanel Height="50" Width="128" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,-4,0,-3">
                <Image Source="img/video1.png" Margin="0,0,0,3" Width="39"/>
                <TextBlock x:Name="TextBlockOpenSource" Margin="0,16,0,0" Width="80" FontFamily="Segoe UI Symbol" Foreground="#FF15417E" FontWeight="Bold">
          <Run Text=" Open "/>
          <Run Text="Source"/>
                </TextBlock>
            </StackPanel>
        </Button>
        <Button x:Name="ButtonSaveVideo" HorizontalAlignment="Left" Margin="164.333,16,0,0" VerticalAlignment="Top" Width="138" Height="47.25" HorizontalContentAlignment="Center" VerticalContentAlignment="Stretch" UseLayoutRounding="False" BorderBrush="#FFE4E4E4" Background="{Binding ElementName=TextBoxPresetName, Path=Background, UpdateSourceTrigger=PropertyChanged}" Click="ButtonSaveVideo_Click">
            <StackPanel Height="46" Width="108" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,0,-3">
                <Image Source="img/save.png" Margin="0,0,0,3" Width="37"/>
                <TextBlock Margin="0,12,0,0" Width="69" FontFamily="Segoe UI Symbol" Foreground="{Binding ElementName=TextBlockOpenSource, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" FontWeight="Bold" Text="Save To..."/>
            </StackPanel>
        </Button>
        <Button x:Name="ButtonStartEncode" HorizontalAlignment="Left" Margin="307.333,16,0,0" VerticalAlignment="Top" Width="121.333" Height="47.25" HorizontalContentAlignment="Center" VerticalContentAlignment="Stretch" UseLayoutRounding="False" BorderBrush="#FFE4E4E4" Background="{Binding ElementName=TextBoxPresetName, Path=Background, UpdateSourceTrigger=PropertyChanged}" Click="ButtonStartEncode_Click">
            <StackPanel Height="44" Width="112" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,-1">
                <Image x:Name="ImageEncode" Source="img/play.png" Margin="0,0,0,3" Width="31"/>
                <TextBlock x:Name="TextBlockEncode" Margin="0,12,0,0" Width="75" FontFamily="Segoe UI Symbol" Foreground="{Binding ElementName=TextBlockOpenSource, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" FontWeight="Bold" Text="Start Encode"/>
            </StackPanel>
        </Button>
        <TabControl x:Name="TabControl" HorizontalAlignment="Left" Height="402" Margin="21,119,0,0" VerticalAlignment="Top" Width="950">
            <TabItem x:Name="TabItem" Header="Overview">
                <Grid x:Name="TabGrid" Background="White">
                    <Label x:Name="LabelVideoSource1" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="Video Source:" HorizontalAlignment="Left" Margin="42,45,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <Label x:Name="LabelVideoSource" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="" HorizontalAlignment="Left" Margin="137,45,0,0" VerticalAlignment="Top" Width="781"/>
                    <Label x:Name="LabelVideoOutput1" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="Video Output:" HorizontalAlignment="Left" Margin="42,76,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <Label x:Name="LabelVideoOutput" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="" HorizontalAlignment="Left" Margin="137,76,0,0" VerticalAlignment="Top" Width="781"/>
                    <Label Content="Encoding Settings:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="41,141,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <Label Content="Encoder:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="164,172,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <Label Content="{Binding ElementName=ComboBoxEncoder, Path=Text, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="225,172,0,0" VerticalAlignment="Top"/>
                    <Label Content="Bit-Depth:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="156,198,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <Label Content="{Binding ElementName=ComboBoxBitDepth, Path=Text, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="225,198,0,0" VerticalAlignment="Top"/>
                    <Label Content="Speed:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="174,224,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <Label Content="{Binding ElementName=SliderPreset, Path=Value, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="225,224,0,0" VerticalAlignment="Top"/>
                    <Label Content="Framerate:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="154,250,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <Label Content="{Binding ElementName=ComboBoxFrameRate, Path=Text, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="225,250,0,0" VerticalAlignment="Top"/>
                </Grid>
            </TabItem>
            <TabItem Header="General Settings">
                <Grid x:Name="TabGrid1" Background="White">
                    <Label Content="Splitting" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label Content="Chunk length:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,45,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <TextBox x:Name="TextBoxChunkLength" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Background="{Binding ElementName=TextBoxPresetName, Path=Background, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="26" Margin="187,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="101" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" PreviewTextInput="NumberValidationTextBox" ToolTip="Splits the Video into n-Seconds Chunks" IsTabStop="False" Text="20" TextChanged="TextBoxChunkLength_TextChanged"/>
                    <Label Content="Re-Encode:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,90,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <ComboBox x:Name="ComboBoxReencodeCodec" HorizontalAlignment="Left" Margin="187,90,0,0" VerticalAlignment="Top" Width="101" Height="26" VerticalContentAlignment="Center">
                        <ComboBoxItem Content="x264"/>
                        <ComboBoxItem Content="utvideo" IsSelected="True"/>
                    </ComboBox>
                    <CheckBox x:Name="CheckBoxReencodeBeforeSplitting" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="Before Splitting" HorizontalAlignment="Left" Margin="311,96,0,0" VerticalAlignment="Top"/>
                    <CheckBox x:Name="CheckBoxReencodeDuringSplitting" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="During Splitting" HorizontalAlignment="Left" Margin="428,96,0,0" VerticalAlignment="Top"/>
                    <Label Content="Workers" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="10,164,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label Content="Parallel Encoding:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,212,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <CheckBox x:Name="CheckBoxChunkLengthAutoCalculation" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="Automatic Calculation" HorizontalAlignment="Left" Margin="311,51,0,0" VerticalAlignment="Top" IsChecked="True"/>
                    <ComboBox x:Name="ComboBoxProcessPriority" HorizontalAlignment="Left" Margin="187,256,0,0" VerticalAlignment="Top" Width="101" Height="26" RenderTransformOrigin="0.495,0.462">
                        <ComboBoxItem Content="Normal" IsSelected="True"/>
                        <ComboBoxItem Content="Below Normal"/>
                    </ComboBox>
                    <Label Content="Process Priority:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,256,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <ComboBox x:Name="ComboBoxWorkers" HorizontalAlignment="Left" Margin="187,212,0,0" VerticalAlignment="Top" Width="101" Height="26" SelectionChanged="ComboBoxWorkers_SelectionChanged"/>
                </Grid>
            </TabItem>
            <TabItem Header="Video">
                <Grid x:Name="TabGrid2" Background="White">
                    <Label Content="Video" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label Content="Encoder:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,45,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <ComboBox x:Name="ComboBoxEncoder" HorizontalAlignment="Left" Margin="135,47,0,0" VerticalAlignment="Top" Width="139" Height="26" SelectionChanged="ComboBoxEncoder_SelectionChanged">
                        <ComboBoxItem Content="aomenc"  IsSelected="True"/>
                        <ComboBoxItem Content="aomenc (ffmpeg)"/>
                        <ComboBoxItem Content="rav1e"/>
                        <ComboBoxItem Content="svt-av1"/>
                    </ComboBox>
                    <Label Content="Framerate:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,84,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <ComboBox x:Name="ComboBoxFrameRate" HorizontalAlignment="Left" Margin="135,86,0,0" VerticalAlignment="Top" Width="69" Height="26" ToolTip="Setting the Framerate won't change the framerate. The Encoder will sync the framerate from the input video.">
                        <ComboBoxItem Content="5"/>
                        <ComboBoxItem Content="10"/>
                        <ComboBoxItem Content="12"/>
                        <ComboBoxItem Content="15"/>
                        <ComboBoxItem Content="20"/>
                        <ComboBoxItem Content="23.976" IsSelected="True"/>
                        <ComboBoxItem Content="24"/>
                        <ComboBoxItem Content="25"/>
                        <ComboBoxItem Content="29.97"/>
                        <ComboBoxItem Content="30"/>
                        <ComboBoxItem Content="48"/>
                        <ComboBoxItem Content="50"/>
                        <ComboBoxItem Content="59.94"/>
                        <ComboBoxItem Content="60"/>
                    </ComboBox>
                    <Label Content="Bit-Depth:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,124,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <ComboBox x:Name="ComboBoxBitDepth" HorizontalAlignment="Left" Margin="135,126,0,0" VerticalAlignment="Top" Width="69" Height="26">
                        <ComboBoxItem Content="8" IsSelected="True"/>
                        <ComboBoxItem Content="10"/>
                        <ComboBoxItem Content="12"/>
                    </ComboBox>
                    <Label Content="Quality" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="385,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Slider x:Name="SliderQuality" HorizontalAlignment="Left" Margin="467,77,0,0" VerticalAlignment="Top" Width="445" TickPlacement="BottomRight" ClipToBounds="True" IsSnapToTickEnabled="True" Height="33" ToolTip="Sets the Constant Quality. It acts like Constant Rate Factor, thus have an dynamic quantizer. " Maximum="63" Value="30" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" />
                    <RadioButton x:Name="RadioButtonConstantQuality" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="Constant Quality:" HorizontalAlignment="Left" Margin="448,51,0,0" VerticalAlignment="Top" IsChecked="True" RenderTransformOrigin="0.527,-0.267"/>
                    <RadioButton x:Name="RadioButtonBitrate" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="Bitrate (kbps):" HorizontalAlignment="Left" Margin="448,133,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="LabelQuality" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="{Binding Value, ElementName=SliderQuality, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="561,45,0,0" VerticalAlignment="Top"/>
                    <TextBox x:Name="TextBoxBitrate"  Background="{Binding ElementName=TextBoxPresetName, Path=Background, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="22" Margin="566,130,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="78" RenderTransformOrigin="0.543,0.45" VerticalContentAlignment="Center" PreviewTextInput="NumberValidationTextBox"/>
                    <Label Content="| Highest Quality" HorizontalAlignment="Left" Margin="466,97,0,0" VerticalAlignment="Top" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" FontSize="11"/>
                    <Label Content="Lowest Quality |" HorizontalAlignment="Left" Margin="825,97,0,0" VerticalAlignment="Top" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" FontSize="11"/>
                    <Label Content="Optimization" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="10,188,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label Content="Speed:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,230,0,0" VerticalAlignment="Top"/>
                    <Slider x:Name="SliderPreset" HorizontalAlignment="Left" Margin="135,235,0,0" VerticalAlignment="Top" Width="139" ClipToBounds="True" TickPlacement="BottomRight" IsSnapToTickEnabled="True" Maximum="8" Value="3" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" />
                    <Label Content="{Binding Value, ElementName=SliderPreset, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="113,230,0,0" VerticalAlignment="Top"/>
                    <Label Content="| Slow" HorizontalAlignment="Left" Margin="134,253,0,0" VerticalAlignment="Top" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" FontSize="11"/>
                    <Label Content="Fast |" HorizontalAlignment="Left" Margin="240,253,0,0" VerticalAlignment="Top" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" FontSize="11"/>
                    <Label Content="Resize" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="385,188,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label Content="Width:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="470,230,0,0" VerticalAlignment="Top" IsEnabled="{Binding ElementName=CheckBoxResize, Path=IsChecked}"/>
                    <Label Content="Height:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="590,230,0,0" VerticalAlignment="Top" IsEnabled="{Binding ElementName=CheckBoxResize, Path=IsChecked}"/>
                    <TextBox x:Name="TextBoxImageWidth" Background="{Binding ElementName=TextBoxPresetName, Path=Background, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="26" Margin="520,230,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="56" VerticalContentAlignment="Center" PreviewTextInput="NumberValidationTextBox" IsEnabled="{Binding ElementName=CheckBoxResize, Path=IsChecked}"/>
                    <TextBox x:Name="TextBoxImageHeight" Background="{Binding ElementName=TextBoxPresetName, Path=Background, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="26" Margin="644,230,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="54" VerticalContentAlignment="Center" PreviewTextInput="NumberValidationTextBox" IsEnabled="{Binding ElementName=CheckBoxResize, Path=IsChecked}"/>
                    <CheckBox x:Name="CheckBoxResize" Content="" HorizontalAlignment="Left" Margin="448,235,0,0" VerticalAlignment="Top"/>
                    <ComboBox x:Name="ComboBoxPasses" HorizontalAlignment="Left" Margin="135,296,0,0" VerticalAlignment="Top" Width="69" Height="24" SelectionChanged="ComboBoxPasses_SelectionChanged" ToolTip="Number of encoding passes. Rav1e Two Pass is broken.">
                        <ComboBoxItem Content="1" IsSelected="True"/>
                        <ComboBoxItem Content="2"/>
                    </ComboBox>
                    <Label Content="Passes:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,294,0,0" VerticalAlignment="Top"/>
                    <CheckBox x:Name="CheckBoxAdvancedSettings" Content="" HorizontalAlignment="Left" Margin="181,338,0,0" VerticalAlignment="Top"/>
                    <Label Content="Advanced Settings:" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,332,0,0" VerticalAlignment="Top"/>
                    <Button Content="Show Command Line" HorizontalAlignment="Left" Margin="805,338,0,0" VerticalAlignment="Top" Width="129" Height="26" Background="{Binding ElementName=TextBoxPresetName, Path=Background, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Click="Button_Click"/>
                </Grid>
            </TabItem>
            <TabItem Header="Advanced Settings" Visibility="{Binding ElementName=CheckBoxAdvancedSettings, Path=IsChecked, Converter={StaticResource b2v}}">
                <Grid x:Name="TabGrid5" Background="{Binding Background, ElementName=TabGrid, UpdateSourceTrigger=PropertyChanged}">
                    <Label Content="Advanced Settings" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <TextBox x:Name="TextBoxAdvancedSettings" IsEnabled="{Binding ElementName=CheckBoxCustomSettings, Path=IsChecked}" Background="{Binding ElementName=TextBoxPresetName, Path=Background, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="48" Margin="10,316,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="924" TextChanged="TextBoxAdvancedSettings_TextChanged" BorderBrush="#FFB3ABAB"/>
                    <Label Content="Threads:" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="38,45,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26">
                        <Label.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger
                      Binding="{Binding SelectedIndex, ElementName=ComboBoxEncoder}" Value="0">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                    <DataTrigger
                      Binding="{Binding SelectedIndex, ElementName=ComboBoxEncoder}" Value="1">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                    <DataTrigger
                      Binding="{Binding SelectedIndex, ElementName=ComboBoxEncoder}" Value="2">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Label.Style>
                    </Label>
                    <ComboBox x:Name="ComboBoxThreadsAomenc" HorizontalAlignment="Left" Margin="138,45,0,0" VerticalAlignment="Top" Width="80" Height="26">
                        <ComboBoxItem Content="1"/>
                        <ComboBoxItem Content="2" IsSelected="True"/>
                        <ComboBoxItem Content="3"/>
                        <ComboBoxItem Content="4"/>
                        <ComboBoxItem Content="5"/>
                        <ComboBoxItem Content="6"/>
                        <ComboBoxItem Content="7"/>
                        <ComboBoxItem Content="8"/>
                        <ComboBox.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger
                      Binding="{Binding SelectedIndex, ElementName=ComboBoxEncoder}" Value="0">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                    <DataTrigger
                      Binding="{Binding SelectedIndex, ElementName=ComboBoxEncoder}" Value="1">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                    <DataTrigger
                      Binding="{Binding SelectedIndex, ElementName=ComboBoxEncoder}" Value="2">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </ComboBox.Style>
                    </ComboBox>
                    <Label Content="Tile Columns:" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="38,76,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <Label Content="Tile Rows:" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}"  HorizontalAlignment="Left" Margin="38,107,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <Label Content="Min GOP Size:" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}"  HorizontalAlignment="Left" Margin="38,138,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26">
                        <Label.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger
                      Binding="{Binding SelectedIndex, ElementName=ComboBoxEncoder}" Value="0">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                    <DataTrigger
                      Binding="{Binding SelectedIndex, ElementName=ComboBoxEncoder}" Value="2">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Label.Style>
                    </Label>
                    <Label Content="Max GOP Size:" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}"  HorizontalAlignment="Left" Margin="37,169,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26">
                        <Label.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger
                      Binding="{Binding SelectedIndex, ElementName=ComboBoxEncoder}" Value="0">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                    <DataTrigger
                      Binding="{Binding SelectedIndex, ElementName=ComboBoxEncoder}" Value="1">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                    <DataTrigger
                      Binding="{Binding SelectedIndex, ElementName=ComboBoxEncoder}" Value="2">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Label.Style>
                    </Label>
                    <TextBox x:Name="TextBoxMinKeyframeinterval" PreviewTextInput="NumberValidationTextBox" Background="{Binding Background, ElementName=TextBoxPresetName, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="26" Margin="138,138,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="80" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" ToolTip="Minimum keyframe interval (frames). This value, expressed as a number of frames, prevents the encoder from placing a keyframe nearer than kf_min_dist to the previous keyframe. Set kf_min_dist equal to kf_max_dist for a fixed interval.">
                        <TextBox.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger
                      Binding="{Binding SelectedIndex, ElementName=ComboBoxEncoder}" Value="0">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                    <DataTrigger
                      Binding="{Binding SelectedIndex, ElementName=ComboBoxEncoder}" Value="2">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </TextBox.Style>
                    </TextBox>
                    <TextBox x:Name="TextBoxMaxKeyframeinterval" PreviewTextInput="NumberValidationTextBox" Background="{Binding Background, ElementName=TextBoxPresetName, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="26" Margin="138,169,0,0" TextWrapping="Wrap" Text="240" VerticalAlignment="Top" Width="80" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" ToolTip="Maximum keyframe interval (frames). A value of 0 implies all frames will be keyframes.">
                        <TextBox.Style>
