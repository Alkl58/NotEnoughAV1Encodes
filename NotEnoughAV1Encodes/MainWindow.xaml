<Window x:Class="NotEnoughAV1Encodes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NyaAV1"
        mc:Ignorable="d"
        Title="NotEnoughAV1Encodes" Height="650" Width="1000" ResizeMode="CanMinimize">
    <Window.Resources>
        <BooleanToVisibilityConverter x:Key="b2v"/>
    </Window.Resources>
    <Grid>
        <Button x:Name="ButtonOpenSource" Margin="21,20,0,0" VerticalAlignment="Top" Height="48" HorizontalAlignment="Left" Width="138" Background="{x:Null}" Click="ButtonOpenSource_Click" BorderBrush="#FF707070">
            <Canvas HorizontalAlignment="Left" Height="40" VerticalAlignment="Top" Width="100">
                <Image HorizontalAlignment="Left" Height="51" VerticalAlignment="Top" Width="48" Source="img/video1.png" Canvas.Top="-5" Canvas.Left="-4"/>
                <Label Content="Source" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.463,0.491" Canvas.Left="44" Height="30" Canvas.Top="4" FontSize="14"/>
            </Canvas>
        </Button>
        <TabControl HorizontalAlignment="Left" Height="439" Margin="21,116,0,0" VerticalAlignment="Top" Width="953">
            <TabItem Header="Summary" FontSize="14">
                <Grid Background="White">
                    <Label Content="Source:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label Content="Destination:" HorizontalAlignment="Left" Margin="10,39,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <TextBox x:Name="TextBoxVideoSource" HorizontalAlignment="Left" Height="29" Margin="105,10,0,0" TextWrapping="Wrap" Text="No Source Selected" VerticalAlignment="Top" Width="480" FontStyle="Italic" VerticalContentAlignment="Center" Foreground="#FF404040" IsReadOnly="True" BorderBrush="{x:Null}"/>
                    <TextBox x:Name="TextBoxVideoDestination" HorizontalAlignment="Left" Height="29" Margin="105,39,0,0" TextWrapping="Wrap" Text="No Destination Selected" VerticalAlignment="Top" Width="480" FontStyle="Italic" VerticalContentAlignment="Center" Foreground="#FF404040" IsReadOnly="True" BorderBrush="{x:Null}"/>
                </Grid>
            </TabItem>
            <TabItem Header="General Settings" FontSize="14">
                <Grid Background="White">
                    <Label Content="Splitting" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label Content="Splitting Method:" HorizontalAlignment="Left" Margin="57,57,0,0" VerticalAlignment="Top"/>
                    <ComboBox x:Name="ComboBoxSplittingMethod" HorizontalAlignment="Left" Margin="179,57,0,0" VerticalAlignment="Top" Width="168" Height="29" VerticalContentAlignment="Center">
                        <ComboBoxItem Content="Scene Detect (ffmpeg)" IsSelected="True"/>
                        <ComboBoxItem Content="PySceneDetect"/>
                        <ComboBoxItem Content="Equal Chunking"/>
                    </ComboBox>
                    <Label Content="Threshold:" HorizontalAlignment="Left" Margin="57,107,0,0" VerticalAlignment="Top">
                        <Label.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="0">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Label.Style>
                    </Label>
                    <TextBox x:Name="TextBoxSplittingThreshold" x:FieldModifier="public" HorizontalAlignment="Left" Height="27" Margin="179,109,0,0" TextWrapping="Wrap" Text="0.3" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">
                        <TextBox.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="0">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </TextBox.Style>
                    </TextBox>
                    <ComboBox x:Name="ComboBoxSplittingReencodeMethod" HorizontalAlignment="Left" Margin="488,57,0,0" VerticalAlignment="Top" Width="80" Height="29" VerticalContentAlignment="Center">
                        <ComboBoxItem Content="x264" IsSelected="True"/>
                        <ComboBoxItem Content="ffv1"/>
                        <ComboBoxItem Content="utvideo"/>
                        <ComboBox.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="2">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </ComboBox.Style>
                    </ComboBox>
                    <CheckBox x:Name="CheckBoxSplittingReencode" Content="Re-Encode" HorizontalAlignment="Left" Margin="397,63,0,0" VerticalAlignment="Top" IsChecked="True">
                        <CheckBox.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="2">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </CheckBox.Style>
                    </CheckBox>
                    <Label Content="Chunk-Length:" HorizontalAlignment="Left" Margin="57,107,0,0" VerticalAlignment="Top">
                        <Label.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="2">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Label.Style>
                    </Label>
                    <TextBox x:Name="TextBoxSplittingChunkLength" HorizontalAlignment="Left" Height="27" Margin="179,109,0,0" TextWrapping="Wrap" Text="10" VerticalAlignment="Top" Width="81" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">
                        <TextBox.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="2">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </TextBox.Style>
                    </TextBox>
                    <Label Content="(in seconds)" HorizontalAlignment="Left" Margin="265,107,0,0" VerticalAlignment="Top" FontStyle="Italic" Foreground="#FF4D4D4D">
                        <Label.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="2">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Label.Style>
                    </Label>
                    <Label Content="Workers" HorizontalAlignment="Left" Margin="10,217,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label Content="Worker Count:" HorizontalAlignment="Left" Margin="57,265,0,0" VerticalAlignment="Top"/>
                    <ComboBox x:Name="ComboBoxWorkerCount" HorizontalAlignment="Left" Margin="179,265,0,0" VerticalAlignment="Top" Width="86" Height="29"/>
                    <Label Content="Process Priority:" HorizontalAlignment="Left" Margin="57,308,0,0" VerticalAlignment="Top"/>
                    <ComboBox x:Name="ComboBoxProcessPriority" HorizontalAlignment="Left" Margin="179,308,0,0" VerticalAlignment="Top" Width="168" Height="29">
                        <ComboBoxItem Content="Normal" IsSelected="True"/>
                        <ComboBoxItem Content="Below Normal"/>
                    </ComboBox>
                </Grid>
            </TabItem>
            <TabItem Header="Filters" FontSize="14">
                <Grid Background="White">
                    <GroupBox Header="Crop" HorizontalAlignment="Left" Height="203" Margin="47,62,0,0" VerticalAlignment="Top" Width="275" FontSize="14"/>
                    <TextBox x:Name="TextBoxFiltersCropLeft" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersCrop}" HorizontalAlignment="Left" Height="27" Margin="96,157,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                    <TextBox x:Name="TextBoxFiltersCropRight" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersCrop}" HorizontalAlignment="Left" Height="27" Margin="207,157,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                    <TextBox x:Name="TextBoxFiltersCropTop" x:FieldModifier="public" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersCrop}" HorizontalAlignment="Left" Height="27" Margin="152,125,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                    <TextBox x:Name="TextBoxFiltersCropBottom" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersCrop}" HorizontalAlignment="Left" Height="27" Margin="152,189,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                    <Label Content="Bottom" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersCrop}" HorizontalAlignment="Left" Margin="152,216,0,0" VerticalAlignment="Top" Width="55"/>
                    <Label Content="Top" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersCrop}" HorizontalAlignment="Left" Margin="164,96,0,0" VerticalAlignment="Top"/>
                    <Label Content="Left" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersCrop}" HorizontalAlignment="Left" Margin="63,157,0,0" VerticalAlignment="Top" Height="27"/>
                    <Label Content="Right" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersCrop}" HorizontalAlignment="Left" Margin="262,156,0,0" VerticalAlignment="Top" Height="30"/>
                    <CheckBox x:Name="CheckBoxFiltersCrop" Content="" HorizontalAlignment="Left" Margin="89,66,0,0" VerticalAlignment="Top"/>
                    <GroupBox Header="Resize" HorizontalAlignment="Left" Height="203" Margin="327,62,0,0" VerticalAlignment="Top" Width="285" FontSize="14">
                        <ComboBox x:Name="ComboBoxFiltersScaling" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersResize}" HorizontalAlignment="Left" Margin="139,74,0,0" VerticalAlignment="Top" Width="120" Height="30" VerticalContentAlignment="Center">
                            <ComboBoxItem Content="fast_bilinear"/>
                            <ComboBoxItem Content="bilinear"/>
                            <ComboBoxItem Content="bicubic" IsSelected="True"/>
                            <ComboBoxItem Content="experimental"/>
                            <ComboBoxItem Content="neighbor"/>
                            <ComboBoxItem Content="area"/>
                            <ComboBoxItem Content="bicublin"/>
                            <ComboBoxItem Content="gauss"/>
                            <ComboBoxItem Content="sinc"/>
                            <ComboBoxItem Content="lanczos"/>
                            <ComboBoxItem Content="spline"/>
                        </ComboBox>
                    </GroupBox>
                    <CheckBox x:Name="CheckBoxFiltersResize" Content="" HorizontalAlignment="Left" Margin="377,66,0,0" VerticalAlignment="Top"/>
                    <Label Content="Width:" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersResize}" HorizontalAlignment="Left" Margin="340,117,0,0" VerticalAlignment="Top" Height="28"/>
                    <Label Content="Height:" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersResize}" HorizontalAlignment="Left" Margin="475,116,0,0" VerticalAlignment="Top" Height="28"/>
                    <TextBox x:Name="TextBoxFiltersResizeWidth" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersResize}" HorizontalAlignment="Left" Height="27" Margin="403,118,0,0" TextWrapping="Wrap" Text="1920" VerticalAlignment="Top" Width="60" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                    <TextBox x:Name="TextBoxFiltersResizeHeight" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersResize}" HorizontalAlignment="Left" Height="27" Margin="535,117,0,0" TextWrapping="Wrap" Text="1080" VerticalAlignment="Top" Width="60" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                    <Label Content="Scaling Algorithm:" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersResize}" HorizontalAlignment="Left" Margin="340,156,0,0" VerticalAlignment="Top" Height="28"/>
                    <GroupBox Header="Rotate" HorizontalAlignment="Left" Height="90" Margin="617,62,0,0" VerticalAlignment="Top" Width="285" FontSize="14"/>
                    <CheckBox x:Name="CheckBoxFiltersRotate" Content="" HorizontalAlignment="Left" Margin="669,66,0,0" VerticalAlignment="Top"/>
                    <Label Content="Rotate (Clockwise):" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersRotate}" HorizontalAlignment="Left" Margin="636,98,0,0" VerticalAlignment="Top" Height="28"/>
                    <ComboBox x:Name="ComboBoxFiltersRotate" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersRotate}" HorizontalAlignment="Left" Margin="767,97,0,0" VerticalAlignment="Top" Width="65" Height="29" VerticalContentAlignment="Center">
                        <ComboBoxItem Content="0°" IsSelected="True"/>
                        <ComboBoxItem Content="90°"/>
                        <ComboBoxItem Content="180°"/>
                        <ComboBoxItem Content="270°"/>
                    </ComboBox>
                    <GroupBox Header="Deinterlace" HorizontalAlignment="Left" Height="95" Margin="617,170,0,0" VerticalAlignment="Top" Width="285" FontSize="14"/>
                    <CheckBox x:Name="CheckBoxFiltersDeinterlace" Content="" HorizontalAlignment="Left" Margin="699,173,0,0" VerticalAlignment="Top"/>
                    <Label Content="Deinterlace (Yadif):" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersDeinterlace}" HorizontalAlignment="Left" Margin="636,209,0,0" VerticalAlignment="Top" Height="28"/>
                    <ComboBox x:Name="ComboBoxFiltersDeinterlace" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersDeinterlace}" HorizontalAlignment="Left" Margin="766,208,0,0" VerticalAlignment="Top" Width="65" Height="29" VerticalContentAlignment="Center">
                        <ComboBoxItem Content="1" IsSelected="True"/>
                        <ComboBoxItem Content="0"/>
                    </ComboBox>
                    <Label Content="Filters" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                </Grid>
            </TabItem>
            <TabItem Header="Video" FontSize="14">
                <Grid Background="White">
                    <Label Content="Video" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label Content="Encoder:" HorizontalAlignment="Left" Margin="57,57,0,0" VerticalAlignment="Top"/>
                    <ComboBox x:Name="ComboBoxVideoEncoder" HorizontalAlignment="Left" Margin="138,57,0,0" VerticalAlignment="Top" Width="127" Height="29" VerticalContentAlignment="Center" SelectionChanged="ComboBoxVideoEncoder_SelectionChanged">
                        <ComboBoxItem Content="aomenc" IsSelected="True"/>
                        <ComboBoxItem Content="rav1e"/>
                        <ComboBoxItem Content="svt-av1"/>
                    </ComboBox>
                    <Label Content="Bit-Depth:" HorizontalAlignment="Left" Margin="57,98,0,0" VerticalAlignment="Top"/>
                    <ComboBox x:Name="ComboBoxVideoBitDepth" HorizontalAlignment="Left" Margin="138,98,0,0" VerticalAlignment="Top" Width="60" Height="29" VerticalContentAlignment="Center">
                        <ComboBoxItem Content="8" IsSelected="True"/>
                        <ComboBoxItem Content="10"/>
                        <ComboBoxItem Content="12"/>
                    </ComboBox>
                    <Label Content="Quality" HorizontalAlignment="Left" Margin="395,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <RadioButton x:Name="RadioButtonVideoConstantQuality" Content="Constant Quality:" HorizontalAlignment="Left" Margin="457,56,0,0" VerticalAlignment="Top" Height="29" RenderTransformOrigin="0.504,0.192" VerticalContentAlignment="Center" IsChecked="True"/>
                    <Slider x:Name="SliderVideoQuality" IsEnabled="{Binding IsChecked, ElementName=RadioButtonVideoConstantQuality}" HorizontalAlignment="Left" Margin="475,90,0,0" VerticalAlignment="Top" Width="431" TickPlacement="BottomRight" Maximum="63" Value="28" SmallChange="1" IsSnapToTickEnabled="True" Height="28" IsDirectionReversed="True" Foreground="#FF595959"/>
                    <Label x:Name="LabelVideoQuality" IsEnabled="{Binding IsChecked, ElementName=RadioButtonVideoConstantQuality}" Content="{Binding Value, ElementName=SliderVideoQuality, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="587,55,0,0" VerticalAlignment="Top"/>
                    <Label Content="| Lowest Quality" HorizontalAlignment="Left" Margin="474,113,0,0" VerticalAlignment="Top" FontSize="9" Width="79" Foreground="#FF595959"/>
                    <Label Content="Highest Quality |" HorizontalAlignment="Left" Margin="831,113,0,0" VerticalAlignment="Top" FontSize="9" Width="79" Foreground="#FF595959"/>
                    <Label Content="Optimisation" HorizontalAlignment="Left" Margin="10,216,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label Content="Speed:" HorizontalAlignment="Left" Margin="57,263,0,0" VerticalAlignment="Top"/>
                    <Slider x:Name="SliderVideoSpeed" HorizontalAlignment="Left" Margin="138,270,0,0" VerticalAlignment="Top" Width="127" Height="29" TickPlacement="BottomRight" Foreground="#FF595959" Maximum="9" Value="4" SmallChange="0" IsSnapToTickEnabled="True"/>
                    <Label x:Name="LabelVideoSpeed" Content="{Binding Value, ElementName=SliderVideoSpeed, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="109,263,0,0" VerticalAlignment="Top"/>
                    <Label Content="Passes:" HorizontalAlignment="Left" Margin="57,323,0,0" VerticalAlignment="Top"/>
                    <ComboBox x:Name="ComboBoxVideoPasses" HorizontalAlignment="Left" Margin="138,323,0,0" VerticalAlignment="Top" Width="60" Height="29" VerticalContentAlignment="Center">
                        <ComboBoxItem Content="1" IsSelected="True"/>
                        <ComboBoxItem Content="2"/>
                    </ComboBox>
                    <Label Content="| Slowest" HorizontalAlignment="Left" Margin="137,292,0,0" VerticalAlignment="Top" FontSize="9" Width="79" Foreground="#FF595959"/>
                    <Label Content="Fastest |" HorizontalAlignment="Left" Margin="224,292,0,0" VerticalAlignment="Top" FontSize="9" Width="79" Foreground="#FF595959"/>
                    <RadioButton x:Name="RadioButtonVideoBitrate" Content="Bitrate (kbps):" HorizontalAlignment="Left" Margin="457,156,0,0" VerticalAlignment="Top" Width="125" Height="29" VerticalContentAlignment="Center"/>
                    <TextBox x:Name="TextBoxVideoBitrate" IsEnabled="{Binding IsChecked, ElementName=RadioButtonVideoBitrate}" HorizontalAlignment="Left" Height="29" Margin="582,156,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                    <CheckBox x:Name="CheckBoxVideoAdvancedSettings" Content="Advanced Settings" HorizontalAlignment="Left" Margin="59,142,0,0" VerticalAlignment="Top" Width="139"/>
                </Grid>
            </TabItem>
            <TabItem Header="Advanced Settings" FontSize="14" Visibility="{Binding ElementName=CheckBoxVideoAdvancedSettings, Path=IsChecked, Converter={StaticResource b2v}}">
                <Grid Background="White">
                    <Label Content="Advanced Settings" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Canvas HorizontalAlignment="Left" Height="237" VerticalAlignment="Top" Width="947" Margin="0,41,0,0">
                        <Canvas.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxVideoEncoder}" Value="2">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Canvas.Style>
                        <Label x:Name="LabelSVTAV1TileColumns" Content="Tile Columns:" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="56" Canvas.Top="17" FontSize="14"/>
                        <Label x:Name="LabelSVTAV1TileRows" Content="Tile Rows:" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="56" Canvas.Top="51" FontSize="14"/>
                        <ComboBox x:Name="ComboBoxSVTAV1TileColumns" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Top" Width="90" Canvas.Left="184" Canvas.Top="17" Height="29" VerticalContentAlignment="Center" RenderTransformOrigin="0.722,0.448">
                            <ComboBoxItem Content="0" IsSelected="True"/>
                            <ComboBoxItem Content="1"/>
                            <ComboBoxItem Content="2"/>
                            <ComboBoxItem Content="3"/>
                            <ComboBoxItem Content="4"/>
                            <ComboBoxItem Content="5"/>
                            <ComboBoxItem Content="6"/>
                        </ComboBox>
                        <ComboBox x:Name="ComboBoxSVTAV1TileRows" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Top" Width="90" Canvas.Left="184" Canvas.Top="51" Height="29" VerticalContentAlignment="Center" RenderTransformOrigin="0.722,0.448">
                            <ComboBoxItem Content="0" IsSelected="True"/>
                            <ComboBoxItem Content="1"/>
                            <ComboBoxItem Content="2"/>
                            <ComboBoxItem Content="3"/>
                            <ComboBoxItem Content="4"/>
                            <ComboBoxItem Content="5"/>
                            <ComboBoxItem Content="6"/>
                        </ComboBox>
                        <Label x:Name="LabelSVTAV1MaxGOP" FontSize="14" Content="Max GOP Size:" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="56" Canvas.Top="85"/>
                        <TextBox x:Name="TextBoxSVTAV1MaxGOP" HorizontalAlignment="Left" Height="29" TextWrapping="Wrap" Text="-2" VerticalAlignment="Top" Width="90" Canvas.Left="184" Canvas.Top="85" FontSize="14" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                        <Label x:Name="LabelSVTAV1Lookahead" FontSize="14" Content="Lookahead:" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="56" Canvas.Top="119"/>
                        <TextBox x:Name="TextBoxSVTAV1Lookahead" HorizontalAlignment="Left" Height="29" TextWrapping="Wrap" Text="33" VerticalAlignment="Top" Width="90" Canvas.Left="184" Canvas.Top="119" FontSize="14" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                        <Label x:Name="LabelSVTAV1AQMode" FontSize="14" Content="AQ-Mode:" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="56" Canvas.Top="153"/>
                        <ComboBox x:Name="ComboBoxSVTAV1AQMode" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Top" Width="90" Canvas.Left="184" Canvas.Top="153" Height="29" VerticalContentAlignment="Center" RenderTransformOrigin="0.722,0.448">
                            <ComboBoxItem Content="0 - OFF" IsSelected="True"/>
                            <ComboBoxItem Content="1 - Variance Base Using Segments"/>
                            <ComboBoxItem Content="2 - Deltaq Pred Efficiency"/>
                        </ComboBox>
                        <Label x:Name="LabelSVTAV1ColorFormat" Content="Color Format:" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="337" Canvas.Top="17" FontSize="14"/>
                        <ComboBox x:Name="ComboBoxSVTAV1ColorFormat" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Top" Width="90" Canvas.Left="452" Canvas.Top="17" Height="29" VerticalContentAlignment="Center" RenderTransformOrigin="0.722,0.448">
                            <ComboBoxItem Content="4:2:0" IsSelected="True"/>
                            <ComboBoxItem Content="4:2:2"/>
                            <ComboBoxItem Content="4:4:4"/>
                        </ComboBox>
                        <Label x:Name="LabelSVTAV1Profile" Content="Profile:" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="337" Canvas.Top="51" FontSize="14"/>
                        <ComboBox x:Name="ComboBoxSVTAV1Profile" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Top" Width="90" Canvas.Left="452" Canvas.Top="51" Height="29" VerticalContentAlignment="Center" RenderTransformOrigin="0.722,0.448">
                            <ComboBoxItem Content="Main" IsSelected="True"/>
                            <ComboBoxItem Content="High"/>
                            <ComboBoxItem Content="Professional"/>
                        </ComboBox>
                        <Label x:Name="LabelSVTAV1AltRefLevel" Content="AltRef Level:" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="337" Canvas.Top="85" FontSize="14"/>
                        <ComboBox x:Name="ComboBoxSVTAV1AltRefLevel" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Top" Width="90" Canvas.Left="452" Canvas.Top="85" Height="29" VerticalContentAlignment="Center" RenderTransformOrigin="0.722,0.448">
                            <ComboBoxItem Content="-1" IsSelected="True"/>
                            <ComboBoxItem Content="0"/>
                            <ComboBoxItem Content="1"/>
                            <ComboBoxItem Content="2"/>
                            <ComboBoxItem Content="3"/>
                        </ComboBox>
                        <Label x:Name="LabelSVTAV1AltRefStrength" Content="AltRef Strength:" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="337" Canvas.Top="119" FontSize="14"/>
                        <ComboBox x:Name="ComboBoxSVTAV1AltRefStrength" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Top" Width="90" Canvas.Left="452" Canvas.Top="119" Height="29" VerticalContentAlignment="Center" RenderTransformOrigin="0.722,0.448">
                            <ComboBoxItem Content="0"/>
                            <ComboBoxItem Content="1"/>
                            <ComboBoxItem Content="2"/>
                            <ComboBoxItem Content="3"/>
                            <ComboBoxItem Content="4"/>
                            <ComboBoxItem Content="5" IsSelected="True"/>
                            <ComboBoxItem Content="6"/>
                        </ComboBox>
                        <Label x:Name="LabelSVTAV1AltRefStrength_Copy" Content="AltRef Frames:" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="337" Canvas.Top="153" FontSize="14"/>
                        <ComboBox x:Name="ComboBoxSVTAV1AltRefFrames" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Top" Width="90" Canvas.Left="452" Canvas.Top="153" Height="29" VerticalContentAlignment="Center" RenderTransformOrigin="0.722,0.448">
                            <ComboBoxItem Content="0"/>
                            <ComboBoxItem Content="1"/>
                            <ComboBoxItem Content="2"/>
                            <ComboBoxItem Content="3"/>
                            <ComboBoxItem Content="4"/>
                            <ComboBoxItem Content="5"/>
                            <ComboBoxItem Content="6"/>
                            <ComboBoxItem Content="7" IsSelected="True"/>
                            <ComboBoxItem Content="8"/>
                            <ComboBoxItem Content="9"/>
                            <ComboBoxItem Content="10"/>
                        </ComboBox>
                        <CheckBox x:Name="CheckBoxSVTAV1HDR" Content="HDR" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="591" Canvas.Top="23"/>
                    </Canvas>
                    <Canvas HorizontalAlignment="Left" Height="237" VerticalAlignment="Top" Width="947" Margin="0,41,0,0">
                        <Canvas.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxVideoEncoder}" Value="1">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
