<Window x:Name="Window" x:Class="NotEnoughAV1Encodes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="NotEnoughAV1Encodes" Height="621" Width="1008.333" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen">
    <Window.Resources>
        <BooleanToVisibilityConverter x:Key="b2v" />
    </Window.Resources>
    <Window.TaskbarItemInfo>
        <TaskbarItemInfo ProgressState="Normal"/>
    </Window.TaskbarItemInfo>

    <Grid x:Name="Grid">
        <Image x:Name="imgDynamic" Stretch="UniformToFill">
        </Image>
        <Button x:Name="ButtonOpenSource" HorizontalAlignment="Left" Margin="21.333,16,0,0" VerticalAlignment="Top" Width="138" Height="47.25" HorizontalContentAlignment="Center" VerticalContentAlignment="Stretch" UseLayoutRounding="False" BorderBrush="#FFE4E4E4" Background="{Binding ElementName=TextBoxChunkLength, Path=Background, UpdateSourceTrigger=PropertyChanged}" Click="ButtonOpenSource_Click">
            <StackPanel Height="50" Width="128" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,-4,0,-3">
                <Image Source="img/video1.png" Margin="0,0,0,3" Width="39"/>
                <TextBlock x:Name="TextBlockOpenSource" Margin="0,16,0,0" Width="80" FontFamily="Segoe UI Symbol" Foreground="#FF15417E" FontWeight="Bold">
          <Run Text=" Open "/>
          <Run Text="Source"/>
                </TextBlock>
            </StackPanel>
        </Button>
        <Button x:Name="ButtonSaveVideo" HorizontalAlignment="Left" Margin="164.333,16,0,0" VerticalAlignment="Top" Width="138" Height="47.25" HorizontalContentAlignment="Center" VerticalContentAlignment="Stretch" UseLayoutRounding="False" BorderBrush="#FFE4E4E4" Background="{Binding ElementName=TextBoxChunkLength, Path=Background, UpdateSourceTrigger=PropertyChanged}" Click="ButtonSaveVideo_Click">
            <StackPanel Height="46" Width="108" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,0,-3">
                <Image Source="img/save.png" Margin="0,0,0,3" Width="37"/>
                <TextBlock Margin="0,12,0,0" Width="69" FontFamily="Segoe UI Symbol" Foreground="{Binding ElementName=TextBlockOpenSource, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" FontWeight="Bold" Text="Save To..."/>
            </StackPanel>
        </Button>
        <Button x:Name="ButtonStartEncode" HorizontalAlignment="Left" Margin="307.333,16,0,0" VerticalAlignment="Top" Width="121.333" Height="47.25" HorizontalContentAlignment="Center" VerticalContentAlignment="Stretch" UseLayoutRounding="False" BorderBrush="#FFE4E4E4" Background="{Binding ElementName=TextBoxChunkLength, Path=Background, UpdateSourceTrigger=PropertyChanged}" Click="ButtonStartEncode_Click">
            <StackPanel Height="44" Width="112" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,-1">
                <Image x:Name="ImageEncode" Source="img/play.png" Margin="0,0,0,3" Width="31"/>
                <TextBlock x:Name="TextBlockEncode" Margin="0,12,0,0" Width="75" FontFamily="Segoe UI Symbol" Foreground="{Binding ElementName=TextBlockOpenSource, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" FontWeight="Bold" Text="Start Encode"/>
            </StackPanel>
        </Button>
        <TabControl x:Name="TabControl" HorizontalAlignment="Left" Height="402" Margin="21,119,0,0" VerticalAlignment="Top" Width="950">
            <TabItem x:Name="TabItem" Header="Overview">
                <Grid x:Name="TabGrid" Background="White">
                    <Label x:Name="LabelVideoSource1" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="Video Source:" HorizontalAlignment="Left" Margin="42,45,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26" FontWeight="Bold"/>
                    <Label x:Name="LabelVideoSource" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="" HorizontalAlignment="Left" Margin="137,45,0,0" VerticalAlignment="Top" Width="731"/>
                    <Label x:Name="LabelVideoOutput1" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="Video Output:" HorizontalAlignment="Left" Margin="42,76,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26" FontWeight="Bold"/>
                    <Label x:Name="LabelVideoOutput" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="" HorizontalAlignment="Left" Margin="137,76,0,0" VerticalAlignment="Top" Width="731"/>
                    <Label Content="Encoding Settings:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="41,141,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26" FontWeight="Bold"/>
                    <Label Content="Encoder:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="164,172,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <Label Content="{Binding ElementName=ComboBoxEncoder, Path=Text, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="225,172,0,0" VerticalAlignment="Top"/>
                    <Label Content="Bit-Depth:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="156,198,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <Label Content="{Binding ElementName=ComboBoxBitDepth, Path=Text, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="225,198,0,0" VerticalAlignment="Top"/>
                    <Label Content="Speed:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="174,224,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <Label Content="{Binding ElementName=SliderPreset, Path=Value, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="225,224,0,0" VerticalAlignment="Top"/>
                    <Label Content="Framerate:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="154,250,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <Label Content="{Binding ElementName=ComboBoxFrameRate, Path=Text, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="225,250,0,0" VerticalAlignment="Top"/>
                </Grid>
            </TabItem>
            <TabItem Header="General Settings">
                <Grid x:Name="TabGrid1" Background="{Binding ElementName=TabGrid,Path=Background,UpdateSourceTrigger=PropertyChanged}">
                    <Label Content="Splitting" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label Content="Chunk length:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,45,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <TextBox x:Name="TextBoxChunkLength" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Background="{Binding ElementName=TextBoxChunkLength, Path=Background, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="26" Margin="187,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="101" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" PreviewTextInput="NumberValidationTextBox" ToolTip="Splits the Video into n-Seconds Chunks. Should not be too short." IsTabStop="False" Text="20" TextChanged="TextBoxChunkLength_TextChanged"/>
                    <Label Content="Re-Encode:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,90,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <ComboBox x:Name="ComboBoxReencodeCodec" HorizontalAlignment="Left" Margin="187,90,0,0" VerticalAlignment="Top" Width="101" Height="26" VerticalContentAlignment="Center" ToolTip="Sets the Codec for reencoding.">
                        <ComboBoxItem Content="x264" IsSelected="True"/>
                        <ComboBoxItem Content="utvideo"/>
                        <ComboBoxItem Content="ffv1"/>
                    </ComboBox>
                    <CheckBox x:Name="CheckBoxReencodeBeforeSplitting" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="Before Splitting" HorizontalAlignment="Left" Margin="311,89,0,0" VerticalAlignment="Top" Unchecked="CheckBoxReencodeBeforeSplitting_Unchecked" ToolTip="Enables Reencoding before the splitting process."/>
                    <CheckBox x:Name="CheckBoxReencodeDuringSplitting" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="During Splitting" HorizontalAlignment="Left" Margin="311,103,0,0" VerticalAlignment="Top" Unchecked="CheckBoxReencodeDuringSplitting_Unchecked" ToolTip="Enables Reencoding during Splitting."/>
                    <Label Content="Workers" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="10,190,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label Content="Parallel Encoding:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,238,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <CheckBox x:Name="CheckBoxChunkLengthAutoCalculation" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="Automatic Calculation" HorizontalAlignment="Left" Margin="311,44,0,0" VerticalAlignment="Top" IsChecked="True"/>
                    <ComboBox x:Name="ComboBoxProcessPriority" HorizontalAlignment="Left" Margin="187,282,0,0" VerticalAlignment="Top" Width="103" Height="26" RenderTransformOrigin="0.495,0.462" ToolTip="Sets the Process Priority of each Worker.">
                        <ComboBoxItem Content="Normal" IsSelected="True"/>
                        <ComboBoxItem Content="Below Normal"/>
                    </ComboBox>
                    <Label Content="Process Priority:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,282,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <ComboBox x:Name="ComboBoxWorkers" HorizontalAlignment="Left" Margin="187,238,0,0" VerticalAlignment="Top" Width="103" Height="26" SelectionChanged="ComboBoxWorkers_SelectionChanged" ToolTip="Sets the amount of Workers which will encode parallel."/>
                    <Label Content="Trim:" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,133,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <TextBox x:Name="TextBoxTrimStart" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" Background="{Binding Background, ElementName=TextBoxChunkLength, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="26" Margin="189,133,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="101" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsTabStop="False" Text="00:00:00.000" TextChanged="TextBoxTrimEnd_TextChanged" BorderBrush="#FF707070" />
                    <Label Content="Start:" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="131,133,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <Label Content="To:" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="141,164,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <TextBox x:Name="TextBoxTrimEnd" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" Background="{Binding Background, ElementName=TextBoxChunkLength, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="26" Margin="189,164,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="101" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsTabStop="False" Text="00:23:00.000" TextChanged="TextBoxTrimEnd_TextChanged" BorderBrush="#FF707070"/>
                    <CheckBox x:Name="CheckBoxTrimming" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" Content="" HorizontalAlignment="Left" Margin="47,139,0,0" VerticalAlignment="Top" Unchecked="CheckBoxTrimming_Unchecked" Checked="CheckBoxTrimming_Checked"/>
                    <Image x:Name="ImagePreviewTrimStart" Visibility="{Binding ElementName=CheckBoxTrimming,Path=IsChecked,Converter={StaticResource b2v}}" HorizontalAlignment="Left" Height="175" Margin="634,10,0,0" VerticalAlignment="Top" Width="286"/>
                    <Image x:Name="ImagePreviewTrimEnd" Visibility="{Binding ElementName=CheckBoxTrimming,Path=IsChecked,Converter={StaticResource b2v}}" HorizontalAlignment="Left" Height="179" Margin="634,185,0,0" VerticalAlignment="Top" Width="286"/>
                    <Label Content="Trim Start:"  Visibility="{Binding ElementName=CheckBoxTrimming,Path=IsChecked,Converter={StaticResource b2v}}"  Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="567,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <Label Content="Trim End:"  Visibility="{Binding ElementName=CheckBoxTrimming,Path=IsChecked,Converter={StaticResource b2v}}"  Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="567,185,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <Button x:Name="ButtonTrimPlus" Content="+" HorizontalAlignment="Left" Margin="290,133,0,0" VerticalAlignment="Top" Width="16" Height="26" Background="{Binding ElementName=ButtonOpenSource, Path=Background, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Click="ButtonTrimPlus_Click"/>
                    <Button x:Name="ButtonTrimMinus" Content="-" HorizontalAlignment="Left" Margin="173,133,0,0" VerticalAlignment="Top" Width="16" Height="26" Background="{Binding Background, ElementName=ButtonOpenSource, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" Click="ButtonTrimMinus_Click"/>
                    <Button x:Name="ButtonTrimPlusEnd" Content="+" HorizontalAlignment="Left" Margin="290,164,0,0" VerticalAlignment="Top" Width="16" Height="26" Background="{Binding Background, ElementName=ButtonOpenSource, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" Click="ButtonTrimPlusEnd_Click" />
                    <Button x:Name="ButtonTrimMinusEnd" Content="-" HorizontalAlignment="Left" Margin="173,164,0,0" VerticalAlignment="Top" Width="16" Height="26" Background="{Binding Background, ElementName=ButtonOpenSource, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" Click="ButtonTrimMinusEnd_Click"  />
                    <CheckBox x:Name="CheckBoxCheckFrameCount" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" Content="Check Frame Count" HorizontalAlignment="Left" Margin="311,58,0,0" VerticalAlignment="Top" IsChecked="True" ToolTip="If enabled: Checks the number of frames before and after splitting. This option is recommended if the reencoding options are deactivated." Checked="CheckBoxCheckFrameCount_Checked" Unchecked="CheckBoxCheckFrameCount_Unchecked"/>
                    <CheckBox x:Name="CheckBoxSplitting" Content="" HorizontalAlignment="Left" Margin="73,17,0,0" VerticalAlignment="Top" ToolTip="Required for parallel encoding!" IsChecked="True" Checked="CheckBoxSplitting_Checked" Unchecked="CheckBoxSplitting_Unchecked"/>
                </Grid>
            </TabItem>
            <TabItem Header="Filters">
                <Grid x:Name="TabGrid10" Background="{Binding ElementName=TabGrid1, Path=Background, UpdateSourceTrigger=PropertyChanged}">
                    <CheckBox x:Name="CheckBoxDeinterlaceYadif" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" Content="Deinterlacing" HorizontalAlignment="Left" Margin="38,60,0,0" VerticalAlignment="Top" Checked="CheckBoxDeinterlaceYadif_Checked" Width="98" FontWeight="Bold"/>
                    <ComboBox x:Name="ComboBoxDeinterlace" HorizontalAlignment="Left" Margin="194,78,0,0" VerticalAlignment="Top" Width="120" Height="26" ToolTip="yadif=0 will drop half the frames, thus resulting in a poor substitute of the original footage.">
                        <ComboBoxItem Content="yadif=0"/>
                        <ComboBoxItem Content="yadif=1" IsSelected="True"/>
                    </ComboBox>
                    <Label Content="Filtering" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label Content="Width:" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="144,161,0,0" VerticalAlignment="Top" IsEnabled="{Binding IsChecked, ElementName=CheckBoxResize}"/>
                    <Label Content="Height:" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="281,161,0,0" VerticalAlignment="Top" IsEnabled="{Binding IsChecked, ElementName=CheckBoxResize}"/>
                    <TextBox x:Name="TextBoxImageWidth" Background="{Binding Background, ElementName=TextBoxChunkLength, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="26" Margin="194,161,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="56" VerticalContentAlignment="Center" PreviewTextInput="NumberValidationTextBox" IsEnabled="{Binding IsChecked, ElementName=CheckBoxResize}"/>
                    <TextBox x:Name="TextBoxImageHeight" Background="{Binding Background, ElementName=TextBoxChunkLength, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="26" Margin="334,161,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="56" VerticalContentAlignment="Center" PreviewTextInput="NumberValidationTextBox" IsEnabled="{Binding IsChecked, ElementName=CheckBoxResize}"/>
                    <CheckBox x:Name="CheckBoxResize" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" Content="Resize" HorizontalAlignment="Left" Margin="38,141,0,0" VerticalAlignment="Top" Width="57" FontWeight="Bold"/>
                    <ComboBox x:Name="ComboBoxResizeFilters" HorizontalAlignment="Left" Margin="537,0,0,184.6" VerticalAlignment="Bottom" Width="108" Height="26">
                        <ComboBoxItem Content="fast_bilinear"/>
                        <ComboBoxItem Content="bilinear"/>
                        <ComboBoxItem Content="bicubic" IsSelected="True"/>
                        <ComboBoxItem Content="experimental"/>
                        <ComboBoxItem Content="neighbor"/>
                        <ComboBoxItem Content="area"/>
                        <ComboBoxItem Content="bicublin"/>
                        <ComboBoxItem Content="gauss"/>
                        <ComboBoxItem Content="sinc"/>
                        <ComboBoxItem Content="lanczos"/>
                        <ComboBoxItem Content="spline"/>
                    </ComboBox>
                    <Label Content="Scaling Algorithm:" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="418,161,0,0" VerticalAlignment="Top" IsEnabled="{Binding IsChecked, ElementName=CheckBoxResize}" Width="106"/>
                    <CheckBox x:Name="CheckBoxCrop" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" Content="Crop" HorizontalAlignment="Left" Margin="38,235,0,0" VerticalAlignment="Top" Width="57" FontWeight="Bold"/>
                    <TextBox x:Name="TextBoxCropTop" Background="{Binding Background, ElementName=TextBoxChunkLength, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="26" Margin="258,235,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="56" VerticalContentAlignment="Center" PreviewTextInput="NumberValidationTextBox" IsEnabled="{Binding IsChecked, ElementName=CheckBoxCrop}" HorizontalContentAlignment="Center" Text="0"/>
                    <TextBox x:Name="TextBoxCropBottom" Background="{Binding Background, ElementName=TextBoxChunkLength, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="26" Margin="258,311,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="56" VerticalContentAlignment="Center" PreviewTextInput="NumberValidationTextBox" IsEnabled="{Binding IsChecked, ElementName=CheckBoxCrop}" HorizontalContentAlignment="Center" Text="0"/>
                    <TextBox x:Name="TextBoxCropLeft" Background="{Binding Background, ElementName=TextBoxChunkLength, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="26" Margin="194,272,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="56" VerticalContentAlignment="Center" PreviewTextInput="NumberValidationTextBox" IsEnabled="{Binding IsChecked, ElementName=CheckBoxCrop}" HorizontalContentAlignment="Center" Text="0"/>
                    <TextBox x:Name="TextBoxCropRight" Background="{Binding Background, ElementName=TextBoxChunkLength, UpdateSourceTrigger=PropertyChanged}" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="26" Margin="323,272,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="56" VerticalContentAlignment="Center" PreviewTextInput="NumberValidationTextBox" IsEnabled="{Binding IsChecked, ElementName=CheckBoxCrop}" HorizontalContentAlignment="Center" Text="0"/>
                    <Label Content="Left" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="159,272,0,0" VerticalAlignment="Top" IsEnabled="{Binding IsChecked, ElementName=CheckBoxResize}"/>
                    <Label Content="Right" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="384,272,0,0" VerticalAlignment="Top" IsEnabled="{Binding IsChecked, ElementName=CheckBoxResize}"/>
                    <Label Content="Bottom" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="258,338,0,0" VerticalAlignment="Top" IsEnabled="{Binding IsChecked, ElementName=CheckBoxResize}" Width="56" HorizontalContentAlignment="Center"/>
                    <Label Content="Top" Foreground="{Binding Foreground, ElementName=LabelPresets, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="258,209,0,0" VerticalAlignment="Top" IsEnabled="{Binding IsChecked, ElementName=CheckBoxResize}" Width="56" HorizontalContentAlignment="Center"/>
                </Grid>
            </TabItem>
            <TabItem Header="Video">
                <Grid x:Name="TabGrid2" Background="{Binding ElementName=TabGrid,Path=Background,UpdateSourceTrigger=PropertyChanged}">
                    <Label Content="Video" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label Content="Encoder:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,45,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <ComboBox x:Name="ComboBoxEncoder" HorizontalAlignment="Left" Margin="135,47,0,0" VerticalAlignment="Top" Width="116" Height="26" SelectionChanged="ComboBoxEncoder_SelectionChanged">
                        <ComboBoxItem Content="aomenc" IsSelected="True"/>
                        <ComboBoxItem Content="libaom"/>
                        <ComboBoxItem Content="rav1e"/>
                        <ComboBoxItem Content="svt-av1"/>
                        <ComboBoxItem Content="libvpx-vp9"/>
                    </ComboBox>
                    <Label Content="Framerate:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,84,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <ComboBox x:Name="ComboBoxFrameRate" HorizontalAlignment="Left" Margin="135,86,0,0" VerticalAlignment="Top" Width="116" Height="26" ToolTip="Does only change the framerate if reencoding is activated." SelectionChanged="ComboBoxFrameRate_SelectionChanged">
                        <ComboBoxItem Content="Same as Source" IsSelected="True"/>
                        <ComboBoxItem Content="5"/>
                        <ComboBoxItem Content="10"/>
                        <ComboBoxItem Content="12"/>
                        <ComboBoxItem Content="15"/>
                        <ComboBoxItem Content="20"/>
                        <ComboBoxItem Content="23.976"/>
                        <ComboBoxItem Content="24"/>
                        <ComboBoxItem Content="25"/>
                        <ComboBoxItem Content="29.97"/>
                        <ComboBoxItem Content="30"/>
                        <ComboBoxItem Content="48"/>
                        <ComboBoxItem Content="50"/>
                        <ComboBoxItem Content="59.94"/>
                        <ComboBoxItem Content="60"/>
                        <ComboBoxItem Content="120"/>
                    </ComboBox>
                    <Label Content="Bit-Depth:" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="65,124,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.442" Height="26"/>
                    <ComboBox x:Name="ComboBoxBitDepth" HorizontalAlignment="Left" Margin="135,126,0,0" VerticalAlignment="Top" Width="69" Height="26">
                        <ComboBoxItem Content="8" IsSelected="True"/>
                        <ComboBoxItem Content="10"/>
                        <ComboBoxItem Content="12">
                            <ComboBoxItem.Style>
                                <Style>
                                    <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                    <Style.Triggers>
                                        <DataTrigger
                      Binding="{Binding SelectedIndex, ElementName=ComboBoxEncoder}" Value="0">
                                            <Setter Property="UIElement.Visibility" Value="Visible"/>
                                        </DataTrigger>
                                        <DataTrigger
                      Binding="{Binding SelectedIndex, ElementName=ComboBoxEncoder}" Value="1">
                                            <Setter Property="UIElement.Visibility" Value="Visible"/>
                                        </DataTrigger>
                                        <DataTrigger
                      Binding="{Binding SelectedIndex, ElementName=ComboBoxEncoder}" Value="4">
                                            <Setter Property="UIElement.Visibility" Value="Visible"/>
                                        </DataTrigger>
                                        <DataTrigger
                      Binding="{Binding SelectedIndex, ElementName=ComboBoxEncoder}" Value="2">
                                            <Setter Property="UIElement.Visibility" Value="Visible"/>
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </ComboBoxItem.Style>
                        </ComboBoxItem>
                    </ComboBox>
                    <Label Content="Quality" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="385,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Slider x:Name="SliderQuality" HorizontalAlignment="Left" Margin="467,77,0,0" VerticalAlignment="Top" Width="445" TickPlacement="BottomRight" ClipToBounds="True" IsSnapToTickEnabled="True" Height="33" ToolTip="Sets the Constant Quality. It acts like Constant Rate Factor, thus have an dynamic quantizer. " Maximum="63" Value="30" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" />
                    <RadioButton x:Name="RadioButtonConstantQuality" Foreground="{Binding ElementName=LabelPresets, Path=Foreground, UpdateSourceTrigger=PropertyChanged}" Content="Constant Quality:" HorizontalAlignment="Left" Margin="448,51,0,0" VerticalAlignment="Top" IsChecked="True" RenderTransformOrigin="0.527,-0.267"/>
