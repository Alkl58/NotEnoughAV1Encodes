<Window x:Class="NotEnoughAV1Encodes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NyaAV1"
        mc:Ignorable="d"
        Title="NotEnoughAV1Encodes" Height="620" Width="1000" ResizeMode="CanMinimize">
    <Grid>
        <Button x:Name="ButtonOpenSource" Margin="21,20,0,0" VerticalAlignment="Top" Height="48" HorizontalAlignment="Left" Width="138" Background="{x:Null}" Click="ButtonOpenSource_Click" BorderBrush="#FFE4E4E4">
            <Canvas HorizontalAlignment="Left" Height="40" VerticalAlignment="Top" Width="100">
                <Image HorizontalAlignment="Left" Height="51" VerticalAlignment="Top" Width="48" Source="img/video1.png" Canvas.Top="-5" Canvas.Left="-4"/>
                <Label Content="Source" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.463,0.491" Canvas.Left="44" Height="30" Canvas.Top="4" FontSize="14"/>
            </Canvas>
        </Button>
        <TabControl HorizontalAlignment="Left" Height="439" Margin="21,92,0,0" VerticalAlignment="Top" Width="953">
            <TabItem Header="Summary" FontSize="14">
                <Grid Background="White">
                    <Label Content="Source:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label x:Name="LabelVideoSource" Content="No Source Selected" HorizontalAlignment="Left" Margin="103,10,0,0" VerticalAlignment="Top" Width="480" FontStyle="Italic" Foreground="#FF404040"/>
                    <Label Content="Destination:" HorizontalAlignment="Left" Margin="10,39,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label x:Name="LabelVideoDestination" Content="No Destination Selected" HorizontalAlignment="Left" Margin="103,39,0,0" VerticalAlignment="Top" Width="480" FontStyle="Italic" Foreground="#FF404040"/>
                </Grid>
            </TabItem>
            <TabItem Header="General Settings" FontSize="14">
                <Grid Background="White">
                    <Label Content="Splitting" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <CheckBox Content="" HorizontalAlignment="Left" Margin="78,18,0,0" VerticalAlignment="Top" IsChecked="True"/>
                    <Label Content="Splitting Method:" HorizontalAlignment="Left" Margin="57,57,0,0" VerticalAlignment="Top"/>
                    <ComboBox x:Name="ComboBoxSplittingMethod" HorizontalAlignment="Left" Margin="179,57,0,0" VerticalAlignment="Top" Width="168" Height="29" VerticalContentAlignment="Center">
                        <ComboBoxItem Content="Scene Detect (ffmpeg)" IsSelected="True"/>
                        <ComboBoxItem Content="PySceneDetect"/>
                        <ComboBoxItem Content="Equal Chunking"/>
                    </ComboBox>
                    <Label Content="Threshold:" HorizontalAlignment="Left" Margin="57,107,0,0" VerticalAlignment="Top">
                        <Label.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="0">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Label.Style>
                    </Label>
                    <TextBox x:Name="TextBoxSplittingThreshold" x:FieldModifier="public" HorizontalAlignment="Left" Height="27" Margin="179,109,0,0" TextWrapping="Wrap" Text="0.3" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">
                        <TextBox.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="0">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </TextBox.Style>
                    </TextBox>
                    <ComboBox x:Name="ComboBoxSplittingReencodeMethod" HorizontalAlignment="Left" Margin="488,57,0,0" VerticalAlignment="Top" Width="80" Height="29" VerticalContentAlignment="Center">
                        <ComboBoxItem Content="x264" IsSelected="True"/>
                        <ComboBoxItem Content="ffv1"/>
                        <ComboBoxItem Content="utvideo"/>
                        <ComboBox.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="2">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </ComboBox.Style>
                    </ComboBox>
                    <CheckBox x:Name="CheckBoxSplittingReencode" Content="Re-Encode" HorizontalAlignment="Left" Margin="397,63,0,0" VerticalAlignment="Top" IsChecked="True">
                        <CheckBox.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="2">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </CheckBox.Style>
                    </CheckBox>
                    <Label Content="Chunk-Length:" HorizontalAlignment="Left" Margin="57,107,0,0" VerticalAlignment="Top">
                        <Label.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="2">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Label.Style>
                    </Label>
                    <TextBox x:Name="TextBoxSplittingChunkLength" HorizontalAlignment="Left" Height="27" Margin="179,109,0,0" TextWrapping="Wrap" Text="10" VerticalAlignment="Top" Width="81" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">
                        <TextBox.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="2">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </TextBox.Style>
                    </TextBox>
                    <Label Content="(in seconds)" HorizontalAlignment="Left" Margin="265,107,0,0" VerticalAlignment="Top" FontStyle="Italic" Foreground="#FF4D4D4D">
                        <Label.Style>
                            <Style>
                                <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SelectedIndex, ElementName=ComboBoxSplittingMethod}" Value="2">
                                        <Setter Property="UIElement.Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Label.Style>
                    </Label>
                    <Label Content="Workers" HorizontalAlignment="Left" Margin="10,217,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label Content="Worker Count:" HorizontalAlignment="Left" Margin="57,265,0,0" VerticalAlignment="Top"/>
                    <ComboBox x:Name="ComboBoxWorkerCount" HorizontalAlignment="Left" Margin="179,265,0,0" VerticalAlignment="Top" Width="86" Height="29"/>
                    <Label Content="Process Priority:" HorizontalAlignment="Left" Margin="57,308,0,0" VerticalAlignment="Top"/>
                    <ComboBox x:Name="ComboBoxProcessPriority" HorizontalAlignment="Left" Margin="179,308,0,0" VerticalAlignment="Top" Width="168" Height="29">
                        <ComboBoxItem Content="Normal" IsSelected="True"/>
                        <ComboBoxItem Content="Below Normal"/>
                    </ComboBox>
                </Grid>
            </TabItem>
            <TabItem Header="Filters" FontSize="14">
                <Grid Background="White">
                    <GroupBox Header="Crop" HorizontalAlignment="Left" Height="203" Margin="10,47,0,0" VerticalAlignment="Top" Width="275" FontSize="14"/>
                    <TextBox x:Name="TextBoxFiltersCropLeft" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersCrop}" HorizontalAlignment="Left" Height="27" Margin="59,142,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                    <TextBox x:Name="TextBoxFiltersCropRight" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersCrop}" HorizontalAlignment="Left" Height="27" Margin="170,142,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                    <TextBox x:Name="TextBoxFiltersCropTop" x:FieldModifier="public" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersCrop}" HorizontalAlignment="Left" Height="27" Margin="115,110,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                    <TextBox x:Name="TextBoxFiltersCropBottom" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersCrop}" HorizontalAlignment="Left" Height="27" Margin="115,174,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                    <Label Content="Bottom" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersCrop}" HorizontalAlignment="Left" Margin="115,201,0,0" VerticalAlignment="Top" Width="55"/>
                    <Label Content="Top" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersCrop}" HorizontalAlignment="Left" Margin="127,81,0,0" VerticalAlignment="Top"/>
                    <Label Content="Left" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersCrop}" HorizontalAlignment="Left" Margin="26,142,0,0" VerticalAlignment="Top" Height="27"/>
                    <Label Content="Right" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersCrop}" HorizontalAlignment="Left" Margin="225,141,0,0" VerticalAlignment="Top" Height="30"/>
                    <CheckBox x:Name="CheckBoxFiltersCrop" Content="" HorizontalAlignment="Left" Margin="52,51,0,0" VerticalAlignment="Top"/>
                    <GroupBox Header="Resize" HorizontalAlignment="Left" Height="203" Margin="290,47,0,0" VerticalAlignment="Top" Width="285" FontSize="14">
                        <ComboBox x:Name="ComboBoxFiltersScaling" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersResize}" HorizontalAlignment="Left" Margin="139,74,0,0" VerticalAlignment="Top" Width="120" Height="30" VerticalContentAlignment="Center">
                            <ComboBoxItem Content="fast_bilinear"/>
                            <ComboBoxItem Content="bilinear"/>
                            <ComboBoxItem Content="bicubic" IsSelected="True"/>
                            <ComboBoxItem Content="experimental"/>
                            <ComboBoxItem Content="neighbor"/>
                            <ComboBoxItem Content="area"/>
                            <ComboBoxItem Content="bicublin"/>
                            <ComboBoxItem Content="gauss"/>
                            <ComboBoxItem Content="sinc"/>
                            <ComboBoxItem Content="lanczos"/>
                            <ComboBoxItem Content="spline"/>
                        </ComboBox>
                    </GroupBox>
                    <CheckBox x:Name="CheckBoxFiltersResize" Content="" HorizontalAlignment="Left" Margin="340,51,0,0" VerticalAlignment="Top"/>
                    <Label Content="Width:" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersResize}" HorizontalAlignment="Left" Margin="303,102,0,0" VerticalAlignment="Top" Height="28"/>
                    <Label Content="Height:" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersResize}" HorizontalAlignment="Left" Margin="438,101,0,0" VerticalAlignment="Top" Height="28"/>
                    <TextBox x:Name="TextBoxFiltersResizeWidth" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersResize}" HorizontalAlignment="Left" Height="27" Margin="366,103,0,0" TextWrapping="Wrap" Text="1920" VerticalAlignment="Top" Width="60" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                    <TextBox x:Name="TextBoxFiltersResizeHeight" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersResize}" HorizontalAlignment="Left" Height="27" Margin="498,102,0,0" TextWrapping="Wrap" Text="1080" VerticalAlignment="Top" Width="60" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                    <Label Content="Scaling Algorithm:" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersResize}" HorizontalAlignment="Left" Margin="303,141,0,0" VerticalAlignment="Top" Height="28"/>
                    <GroupBox Header="Rotate" HorizontalAlignment="Left" Height="90" Margin="580,47,0,0" VerticalAlignment="Top" Width="285" FontSize="14"/>
                    <CheckBox x:Name="CheckBoxFiltersRotate" Content="" HorizontalAlignment="Left" Margin="632,51,0,0" VerticalAlignment="Top"/>
                    <Label Content="Rotate (Clockwise):" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersRotate}" HorizontalAlignment="Left" Margin="599,83,0,0" VerticalAlignment="Top" Height="28"/>
                    <ComboBox x:Name="ComboBoxFiltersRotate" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersRotate}" HorizontalAlignment="Left" Margin="730,82,0,0" VerticalAlignment="Top" Width="65" Height="29" VerticalContentAlignment="Center">
                        <ComboBoxItem Content="0°" IsSelected="True"/>
                        <ComboBoxItem Content="90°"/>
                        <ComboBoxItem Content="180°"/>
                        <ComboBoxItem Content="270°"/>
                    </ComboBox>
                    <GroupBox Header="Deinterlace" HorizontalAlignment="Left" Height="95" Margin="580,155,0,0" VerticalAlignment="Top" Width="285" FontSize="14"/>
                    <CheckBox x:Name="CheckBoxFiltersDeinterlace" Content="" HorizontalAlignment="Left" Margin="662,158,0,0" VerticalAlignment="Top"/>
                    <Label Content="Deinterlace (Yadif):" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersDeinterlace}" HorizontalAlignment="Left" Margin="599,194,0,0" VerticalAlignment="Top" Height="28"/>
                    <ComboBox x:Name="ComboBoxFiltersDeinterlace" IsEnabled="{Binding IsChecked, ElementName=CheckBoxFiltersDeinterlace}" HorizontalAlignment="Left" Margin="729,193,0,0" VerticalAlignment="Top" Width="65" Height="29" VerticalContentAlignment="Center">
                        <ComboBoxItem Content="1" IsSelected="True"/>
                        <ComboBoxItem Content="0"/>
                    </ComboBox>
                    <Label Content="Filters" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                </Grid>
            </TabItem>
            <TabItem Header="Video" FontSize="14">
                <Grid Background="White">
                    <Label Content="Video" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label Content="Encoder:" HorizontalAlignment="Left" Margin="57,57,0,0" VerticalAlignment="Top"/>
                    <ComboBox x:Name="ComboBoxVideoEncoder" HorizontalAlignment="Left" Margin="138,57,0,0" VerticalAlignment="Top" Width="127" Height="29" VerticalContentAlignment="Center">
                        <ComboBoxItem Content="aomenc" IsSelected="True"/>
                        <ComboBoxItem Content="rav1e"/>
                        <ComboBoxItem Content="svt-av1"/>
                        <ComboBoxItem Content="libvpx-vp9"/>
                        <ComboBoxItem Content="libx265"/>
                    </ComboBox>
                    <Label Content="Framerate:" HorizontalAlignment="Left" Margin="57,102,0,0" VerticalAlignment="Top"/>
                    <ComboBox x:Name="ComboBoxVideoFramerate" HorizontalAlignment="Left" Margin="138,102,0,0" VerticalAlignment="Top" Width="127" Height="29" VerticalContentAlignment="Center">
                        <ComboBoxItem Content="Same as Source" IsSelected="True"/>
                        <ComboBoxItem Content="5"/>
                        <ComboBoxItem Content="10"/>
                        <ComboBoxItem Content="12"/>
                        <ComboBoxItem Content="15"/>
                        <ComboBoxItem Content="20"/>
                        <ComboBoxItem Content="23.976"/>
                        <ComboBoxItem Content="24"/>
                        <ComboBoxItem Content="25"/>
                        <ComboBoxItem Content="29.97"/>
                        <ComboBoxItem Content="30"/>
                        <ComboBoxItem Content="48"/>
                        <ComboBoxItem Content="50"/>
                        <ComboBoxItem Content="59.94"/>
                        <ComboBoxItem Content="60"/>
                        <ComboBoxItem Content="75"/>
                        <ComboBoxItem Content="120"/>
                        <ComboBoxItem Content="144"/>
                        <ComboBoxItem Content="240"/>
                    </ComboBox>
                    <Label Content="Bit-Depth:" HorizontalAlignment="Left" Margin="57,147,0,0" VerticalAlignment="Top"/>
                    <ComboBox x:Name="ComboBoxVideoBitDepth" HorizontalAlignment="Left" Margin="138,147,0,0" VerticalAlignment="Top" Width="60" Height="29" VerticalContentAlignment="Center">
                        <ComboBoxItem Content="8" IsSelected="True"/>
                        <ComboBoxItem Content="10"/>
                        <ComboBoxItem Content="12"/>
                    </ComboBox>
                    <Label Content="Quality" HorizontalAlignment="Left" Margin="395,10,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <RadioButton x:Name="RadioButtonVideoConstantQuality" Content="Constant Quality:" HorizontalAlignment="Left" Margin="457,56,0,0" VerticalAlignment="Top" Height="29" RenderTransformOrigin="0.504,0.192" VerticalContentAlignment="Center" IsChecked="True"/>
                    <Slider x:Name="SliderVideoQuality" IsEnabled="{Binding IsChecked, ElementName=RadioButtonVideoConstantQuality}" HorizontalAlignment="Left" Margin="475,90,0,0" VerticalAlignment="Top" Width="431" TickPlacement="BottomRight" Maximum="63" Value="28" SmallChange="1" IsSnapToTickEnabled="True" Height="28" IsDirectionReversed="True" Foreground="#FF595959"/>
                    <Label x:Name="LabelVideoQuality" IsEnabled="{Binding IsChecked, ElementName=RadioButtonVideoConstantQuality}" Content="{Binding Value, ElementName=SliderVideoQuality, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="587,55,0,0" VerticalAlignment="Top"/>
                    <Label Content="| Lowest Quality" HorizontalAlignment="Left" Margin="474,113,0,0" VerticalAlignment="Top" FontSize="9" Width="79" Foreground="#FF595959"/>
                    <Label Content="Highest Quality |" HorizontalAlignment="Left" Margin="831,113,0,0" VerticalAlignment="Top" FontSize="9" Width="79" Foreground="#FF595959"/>
                    <Label Content="Optimisation" HorizontalAlignment="Left" Margin="10,216,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
                    <Label Content="Speed:" HorizontalAlignment="Left" Margin="57,263,0,0" VerticalAlignment="Top"/>
                    <Slider x:Name="SliderVideoSpeed" HorizontalAlignment="Left" Margin="138,270,0,0" VerticalAlignment="Top" Width="127" Height="29" TickPlacement="BottomRight" Foreground="#FF595959" Maximum="9" Value="4" SmallChange="0" IsSnapToTickEnabled="True"/>
                    <Label x:Name="LabelVideoSpeed" Content="{Binding Value, ElementName=SliderVideoSpeed, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="109,263,0,0" VerticalAlignment="Top"/>
                    <Label Content="Passes:" HorizontalAlignment="Left" Margin="57,323,0,0" VerticalAlignment="Top"/>
                    <ComboBox x:Name="ComboBoxVideoPasses" HorizontalAlignment="Left" Margin="138,323,0,0" VerticalAlignment="Top" Width="60" Height="29" VerticalContentAlignment="Center">
                        <ComboBoxItem Content="1" IsSelected="True"/>
                        <ComboBoxItem Content="2"/>
                    </ComboBox>
                    <Label Content="| Slowest" HorizontalAlignment="Left" Margin="137,292,0,0" VerticalAlignment="Top" FontSize="9" Width="79" Foreground="#FF595959"/>
                    <Label Content="Fastest |" HorizontalAlignment="Left" Margin="224,292,0,0" VerticalAlignment="Top" FontSize="9" Width="79" Foreground="#FF595959"/>
                    <RadioButton x:Name="RadioButtonVideoBitrate" Content="Bitrate (kbps):" HorizontalAlignment="Left" Margin="457,156,0,0" VerticalAlignment="Top" Width="125" Height="29" VerticalContentAlignment="Center"/>
                    <TextBox x:Name="TextBoxVideoBitrate" IsEnabled="{Binding IsChecked, ElementName=RadioButtonVideoBitrate}" HorizontalAlignment="Left" Height="29" Margin="582,156,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                </Grid>
            </TabItem>
            <TabItem Header="Audio" FontSize="14">
                <Grid Background="White"/>
            </TabItem>
            <TabItem Header="Subtitles" FontSize="14">
                <Grid Background="White"/>
            </TabItem>
        </TabControl>
        <Button x:Name="ButtonOpenDestination" Margin="164,20,0,0" VerticalAlignment="Top" Height="48" HorizontalAlignment="Left" Width="138" Background="{x:Null}" Click="ButtonOpenDestination_Click" BorderBrush="#FFE4E4E4">
            <Canvas HorizontalAlignment="Left" Height="40" VerticalAlignment="Top" Width="100">
                <Image HorizontalAlignment="Left" Height="51" VerticalAlignment="Top" Width="48" Source="img/save.png" Canvas.Top="-5" Canvas.Left="-9"/>
                <Label Content="Destination" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.463,0.491" Canvas.Left="34" Height="30" Canvas.Top="5" FontSize="14"/>
            </Canvas>
        </Button>
        <Button x:Name="ButtonStartEncode" Margin="307,20,0,0" VerticalAlignment="Top" Height="48" HorizontalAlignment="Left" Width="135" Background="{x:Null}" BorderBrush="#FFE4E4E4" Click="ButtonStartEncode_Click">
            <Canvas HorizontalAlignment="Left" Height="40" VerticalAlignment="Top" Width="100">
                <Image HorizontalAlignment="Left" Height="47" VerticalAlignment="Top" Width="47" Source="img/play.png" Canvas.Top="-4" Canvas.Left="-14"/>
                <Label Content="Start Encode" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.463,0.491" Canvas.Left="26" Height="30" Canvas.Top="4" FontSize="14"/>
            </Canvas>
        </Button>
        <ProgressBar x:Name="ProgressBar" HorizontalAlignment="Left" Height="34" Margin="21,536,0,0" VerticalAlignment="Top" Width="953"/>
    </Grid>
</Window>
