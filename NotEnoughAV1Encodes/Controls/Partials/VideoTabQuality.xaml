                </Canvas>
                <!-- VP9 FFmpeg -->
                <Canvas HorizontalAlignment="Left" Width="1055">
                    <Canvas.Style>
                        <Style>
                            <Setter Property="UIElement.Visibility" Value="Collapsed" />
